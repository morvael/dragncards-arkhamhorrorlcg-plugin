            ],
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer4", "A"],
                ["UPDATE_VAR", "$ABOVE_LOCATIONS", ["APPEND", "$ABOVE_LOCATIONS", "Balcony"]]
            ],
            ["COND",
                ["GREATER_THAN", ["LENGTH", "$ABOVE_LOCATIONS"], 0],
