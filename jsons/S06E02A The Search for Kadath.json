                    ],
                    ["DO_SET_CARD_LOCATION", "$CARD.id", "$SPAWN_LEFT", 10],
                    ["INCREASE_VAR", "$SPAWN_LEFT", 5]
                ]],
                ["VAR", "$SELECTED_COUNT", ["LENGTH", "$SELECTED_CARDS"]],
                ["COND",
                    ["GREATER_THAN", "$SELECTED_COUNT", 0],
                    ["DO_FSYSLOG", "Move each Tenebrous Nightgaunt to any City location.", ["LIST"]]
                ],
