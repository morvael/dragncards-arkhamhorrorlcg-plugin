    "functions": {
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_PRE_INIT": {
            "args": ["$CAMPAIGN_NAME", "$SCENARIO_NAME", "$SCENARIO_ACTION", "$OBSCURING_FOG_NAME", "$TERROR_IN_THE_NIGHT_NAME", "$WRAITH_NAME"],
            "code": [
                ["SET", "/arkhamDisappearanceAtTheTwilightEstateCampaignName", "$CAMPAIGN_NAME"],
                ["SET", "/arkhamDisappearanceAtTheTwilightEstateScenarioName", "$SCENARIO_NAME"],
                ["SET", "/arkhamDisappearanceAtTheTwilightEstateScenarioAction", "$SCENARIO_ACTION"],
                ["SET", "/arkhamDisappearanceAtTheTwilightEstateObscuringFogName", "$OBSCURING_FOG_NAME"],
                ["SET", "/arkhamDisappearanceAtTheTwilightEstateTerrorInTheNightName", "$TERROR_IN_THE_NIGHT_NAME"],
                ["SET", "/arkhamDisappearanceAtTheTwilightEstateWraithName", "$WRAITH_NAME"],
                ["VAR", "$SELECTED_CARDS", ["FILTER_CARDS", "$CARD", ["AND",
                    ["IN_STRING", "$CARD.controller", "player"]
                ]]],
                ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                    ["DELETE_CARD", "$CARD.id"]
                ]],
                ["DO_FSYSLOG", "For the duration of this prologue, you will control one of four neutral investigators instead of your chosen one{0:S}.", ["LIST", "$GAME.numPlayers"]],
                ["SET", "/arkhamSkipDeckCheck", true],
                ["DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SHOW_INVESTIGATOR_CHOICE", "$GAME.firstPlayer"]
            ]
        },
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SHOW_INVESTIGATOR_CHOICE": {
            "args": ["$TARGET_PLAYER"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SHOW_INVESTIGATOR_CHOICE.TARGET_PLAYER"],
                ["VAR", "$GAVRIELLA", ["GET_NAMED_CARD", "Gavriella Mizrah"]],
                ["VAR", "$JEROME", ["GET_NAMED_CARD", "Jerome Davids"]],
                ["VAR", "$VALENTINO", ["GET_NAMED_CARD", "Valentino Rivas"]],
                ["VAR", "$PENNY", ["GET_NAMED_CARD", "Penny White"]],
                ["PROMPT", "$TARGET_PLAYER", "disappearanceAtTheTwilightEstateChooseInvestigator"],
                ["COND",
                    ["EQUAL", "$GAVRIELLA", null],
                    ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "Gavriella Mizrah: Private Security", "G", ["LIST", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE", "Gavriella Mizrah: Private Security", "05046"]]
                ],
                ["COND",
                    ["EQUAL", "$JEROME", null],
                    ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "Jerome Davids: Josef's Secretary", "J", ["LIST", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE", "Jerome Davids: Josef's Secretary", "05047"]]
                ],
                ["COND",
                    ["EQUAL", "$VALENTINO", null],
                    ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "Valentino Rivas: Wealthy Philanthropist", "V", ["LIST", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE", "Valentino Rivas: Wealthy Philanthropist", "05048"]]
                ],
                ["COND",
                    ["EQUAL", "$PENNY", null],
                    ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "Penny White: Josef's Housekeeper", "P", ["LIST", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE", "Penny White: Josef's Housekeeper", "05049"]]
                ]
            ]
        },
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE": {
            "args": ["$INVESTIGATOR_NAME", "$INVESTIGATOR_DATABASE_ID"],
            "code": [
                ["VALIDATE_NOT_EMPTY", "$INVESTIGATOR_NAME", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE.INVESTIGATOR_NAME"],
                ["VALIDATE_NOT_EMPTY", "$INVESTIGATOR_DATABASE_ID", "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_INVESTIGATOR_CHOICE_DONE.INVESTIGATOR_DATABASE_ID"],
                ["DO_CREATE_CARDS", "$PLAYER_N", "$INVESTIGATOR_NAME", "$INVESTIGATOR_DATABASE_ID", 1, ["GET_CONTROLLER_INVESTIGATOR", "$PLAYER_N"], true, "$PLAYER_N"],
                ["VAR", "$NEXT_PLAYER", ["NEXT_PLAYER", "$PLAYER_N"]],
                ["COND",
                    ["EQUAL", "$NEXT_PLAYER", "$GAME.firstPlayer"],
                    ["SCENARIO_INIT", "The Circle Undone", "$GAME.arkhamDisappearanceAtTheTwilightEstateCampaignName", "$GAME.arkhamDisappearanceAtTheTwilightEstateScenarioName", "Disappearance at the Twilight Estate Scenario Early Init", "$GAME.arkhamDisappearanceAtTheTwilightEstateScenarioAction"],
                    ["TRUE"],
                    ["DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SHOW_INVESTIGATOR_CHOICE", "$NEXT_PLAYER"]
                ]
            ]
        },
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SETUP_GAVRIELLA": {
            "args": [],
            "code": [
                ["VAR", "$GAVRIELLA", ["GET_NAMED_CARD", "Gavriella Mizrah"]],
                ["COND",
                    ["NOT_EQUAL", "$GAVRIELLA", null],
                    [
                        ["VAR", "$PLAYER_N", "$GAVRIELLA.controller"],
                        //Setup: You begin play in the Victorian Halls. Place 1 damage on The Spectral Watcher. Search the gathered cards for 1 copy of Fate of All Fools and put it into play in your threat area. You begin with 1 resource instead of 5.
                        //Starting Play Area: .45 Automatic (Core 16) with 2 ammo tokens remaining, Physical Training (Core 17).
                        //Opening Hand: First Aid (Core 19), Guard Dog (Core 21), Evidence! (Core 22), Dodge (Core 23), Extra Ammunition (level 1) (Core 26), 2 copies of Delay the Inevitable (TCU 21).
                        ["SET", "/playerData/$PLAYER_N/arkhamCustomSpawnLocation", "Victorian Halls"],
                        ["VAR", "$THE_SPECTRAL_WATCHER", ["GET_NAMED_CARD", "The Spectral Watcher"]],
                        ["DO_PLACE_DAMAGE_ON_CARD", "$PLAYER_N", "$THE_SPECTRAL_WATCHER.id", 1, true, null],
                        ["DO_MOVE_NAMED_DECK_CARDS", "$PLAYER_N", "Fate of All Fools", "sharedEncounterDeck", 1, ["GET_CONTROLLER_THREAT", "$PLAYER_N"], true, null],
                        ["DO_LOAD_CARDS", "$PLAYER_N", "Disappearance at the Twilight Estate - Gavriella Mizrah", true, null],
                        ["VAR", "$DOT_45_AUTOMATIC", ["GET_NAMED_DECK_CARD", ".45 Automatic", ["GET_CONTROLLER_PLAY", "$PLAYER_N"]]],
                        ["DO_PLACE_RESOURCES_ON_CARD", "$PLAYER_N", "$DOT_45_AUTOMATIC.id", 2, true, null],
                        ["SET", "/cardById/$DOT_45_AUTOMATIC.id/usesPlaced", true]
                    ]
                ]
            ]
        },
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SETUP_JEROME": {
            "args": [],
            "code": [
                ["VAR", "$JEROME", ["GET_NAMED_CARD", "Jerome Davids"]],
                ["COND",
                    ["NOT_EQUAL", "$JEROME", null],
                    [
                        ["VAR", "$PLAYER_N", "$JEROME.controller"],
                        //Setup: You begin play in the Office. Search the gathered cards for 1 copy of Nether Mist and 1 copy of Obscuring Fog. Spawn the Nether Mist in the Office and attach Obscuring Fog to the Office. You begin with 3 resources instead of 5.
                        //Starting Play Area: Nothing.
                        //Opening Hand: Hyperawareness (Core 34), Mind over Matter (Core 36), Working a Hunch (Core 37), Barricade (Core 38), Deduction (Core 39), Magnifying Glass (level 1) (Core 40), Fingerprint Kit (TCU 24), Connect the Dots (TCU 25), 2 copies of Curiosity (TCU 26).
                        ["SET", "/playerData/$PLAYER_N/arkhamCustomSpawnLocation", "Office"],
                        ["DO_SPAWN_ENCOUNTER_AT_LOCATION", "$PLAYER_N", "Nether Mist", "Office", 3, 3, 0, true, null],
                        ["DO_MOVE_NAMED_DECK_CARDS", "$PLAYER_N", "$GAME.arkhamDisappearanceAtTheTwilightEstateObscuringFogName", "sharedEncounterDeck", 1, "sharedLocations", true, null],
                        ["VAR", "$OFFICE", ["GET_NAMED_MAP_LOCATION", "Office"]],
                        ["DO_ATTACH_CARD", "$PLAYER_N", "$DO_MOVE_NAMED_DECK_CARDS_RESULT.[0]", "$OFFICE.id", true, null],
                        ["DO_LOAD_CARDS", "$PLAYER_N", "Disappearance at the Twilight Estate - Jerome Davids", true, null]
                    ]
                ]
            ]
        },
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SETUP_VALENTINO": {
            "args": [],
            "code": [
                ["VAR", "$VALENTINO", ["GET_NAMED_CARD", "Valentino Rivas"]],
                ["COND",
                    ["NOT_EQUAL", "$VALENTINO", null],
                    [
                        ["VAR", "$PLAYER_N", "$VALENTINO.controller"],
                        //Setup: You begin play in the Billiards Room. Search the gathered cards for 1 copy of Shadow Hound and 1 copy of Terror in the Night. Spawn the Shadow Hound engaged with you and put Terror in the Night into play next to the agenda deck. You begin with 10 resources instead of 5.
                        //Starting Play Area: Well Connected (TCU 28).
                        //Opening Hand: .41 Derringer (Core 47), Opportunist (Core 53), Sure Gamble (level 3) (Core 56), 2 copies of Money Talks (TCU 29), 2 copies of Cunning (TCU 30).
                        ["SET", "/playerData/$PLAYER_N/arkhamCustomSpawnLocation", "Billiards Room"],
                        ["DO_MOVE_NAMED_DECK_CARDS", "$PLAYER_N", "Shadow Hound", "sharedEncounterDeck", 1, ["GET_CONTROLLER_THREAT", "$PLAYER_N"], true, null],
                        ["DO_MOVE_NAMED_DECK_CARDS", "$PLAYER_N", "$GAME.arkhamDisappearanceAtTheTwilightEstateTerrorInTheNightName", "sharedEncounterDeck", 1, "sharedOffMap", true, null],
                        ["DO_LOAD_CARDS", "$PLAYER_N", "Disappearance at the Twilight Estate - Valentino Rivas", true, null]
                    ]
                ]
            ]
        },
        "DISAPPEARANCE_AT_THE_TWILIGHT_ESTATE_SETUP_PENNY": {
            "args": [],
            "code": [
                ["VAR", "$PENNY", ["GET_NAMED_CARD", "Penny White"]],
                ["COND",
                    ["NOT_EQUAL", "$PENNY", null],
                    [
                        ["VAR", "$PLAYER_N", "$PENNY.controller"],
                        //Setup: You begin play at the Balcony. Search the gathered cards for 1 copy of Wraith and 1 copy of Whispers in the Dark. Spawn the Wraith engaged with you and put Whispers in the Dark into play next to the agenda deck. You begin with 2 resources instead of 5.
                        //Starting Play Area: Dig Deep (Core 77), Knife (Core 86), Flashlight (Core 87) with 2 supply tokens remaining.
                        //Opening Hand: Stray Cat (Core 76), Lucky! (Core 80), Knife (Core 86), Flashlight (Core 87), 2 copies of Act of Desperation (TCU 37), 2 copies of Able Bodied (TCU 38).
                        ["SET", "/playerData/$PLAYER_N/arkhamCustomSpawnLocation", "Balcony"],
                        ["DO_MOVE_NAMED_DECK_CARDS", "$PLAYER_N", "$GAME.arkhamDisappearanceAtTheTwilightEstateWraithName", "sharedEncounterDeck", 1, ["GET_CONTROLLER_THREAT", "$PLAYER_N"], true, null],
                        ["DO_MOVE_NAMED_DECK_CARDS", "$PLAYER_N", "Whispers in the Dark", "sharedEncounterDeck", 1, "sharedOffMap", true, null],
                        ["DO_LOAD_CARDS", "$PLAYER_N", "Disappearance at the Twilight Estate - Penny White", true, null],
                        ["VAR", "$FLASHLIGHT", ["GET_NAMED_DECK_CARD", "Flashlight", ["GET_CONTROLLER_PLAY", "$PLAYER_N"]]],
                        ["DO_PLACE_RESOURCES_ON_CARD", "$PLAYER_N", "$FLASHLIGHT.id", 2, true, null],
                        ["SET", "/cardById/$FLASHLIGHT.id/usesPlaced", true]
                    ]
                ]
            ]
        }
    },
