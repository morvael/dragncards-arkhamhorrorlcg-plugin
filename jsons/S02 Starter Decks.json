    "automation": {
        "cards": {
            "02003": { //Jenny Barnes: The Dilettante
                "rules": {
                    "02003a": { //You collect 1 additional resource during each upkeep phase.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay", "/cardById/$THIS_ID/controller"],
                        "condition": ["CAN_CHANGE_CONTROLLER_BONUS", "$THIS", "arkhamUpkeepResourcesMod", 1, "$THIS.inPlay"],
                        "then": [
                            ["DO_CHANGE_CONTROLLER_BONUS", "$THIS", "arkhamUpkeepResourcesMod", 1, "$THIS.inPlay", "you collect 1 additional resource during each upkeep phase.", null]
                        ]
                    }
                }
            },
            "98001": { //Jenny Barnes: The Dilettante (promo)
