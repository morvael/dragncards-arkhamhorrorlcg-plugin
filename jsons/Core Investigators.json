                            ["UPDATE_VAR", "$SUFFIX", "{{$SUFFIX}} chaos symbol effect)"]
                        ],
                        ["DO_LOG", "$LOG_EVENT", "$ACTOR_PLAYER", "$TARGET_PLAYER", "calculate {{$TARGET_CARD.currentFace.name}}'s {{$SKILL_NAME}} value: {{$SUM_SKILL}}{{$SUFFIX}}.", "calculates {{$TARGET_CARD.currentFace.name}}'s {{$SKILL_NAME}} value: {{$SUM_SKILL}}{{$SUFFIX}}."]
                    ]
                ]
            ]
