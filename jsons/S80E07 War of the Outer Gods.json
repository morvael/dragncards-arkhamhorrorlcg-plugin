    "actionLists": {
        "War of the Outer Gods Scenario Early Init": [
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Decide.", ["LIST", "Play in Epic Multiplayer Mode.", "Play in Single Group Mode."], "War of the Outer Gods Scenario Question 1"]
        ],
        "War of the Outer Gods Scenario Question 1": [
            ["LOG", "└── Decision: {{$GAME.arkham.scenarioAnswer1Text}}"],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"],
                ["CALCULATOR_PROMPT", "$GAME.firstPlayer", "Enter number of players", ["ADD", "$GAME.arkham.numPlayers", 1], 1000, 0, "WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK", "$GAME.firstPlayer", "", ""],
                ["TRUE"],
                ["WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK", "$GAME.firstPlayer", "", "└── ", "$GAME.firstPlayer", "{{$GAME.arkham.numPlayers}}"]
            ]
        ],
        "War of the Outer Gods Scenario Question 2": [
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"],
                ["CALCULATOR_PROMPT", "$GAME.firstPlayer", "Enter number of groups", ["DIVIDE_ROUNDING_UP", "$GAME.arkham.scenarioWarOfTheOuterGodsNumberOfPlayers", 4], "$GAME.arkham.scenarioWarOfTheOuterGodsNumberOfPlayers", 0, "WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK", "$GAME.firstPlayer", "", ""],
                ["TRUE"],
                ["WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK", "$GAME.firstPlayer", "", "└── ", "$GAME.firstPlayer", "1"]
            ]
        ],
        "War of the Outer Gods Scenario Init": [
            ["VAR", "$DOOM", ["MULTIPLY", "$GAME.arkham.scenarioWarOfTheOuterGodsNumberOfGroups", 6]],
            ["VAR", "$HEALTH8", ["MULTIPLY", "$GAME.arkham.scenarioWarOfTheOuterGodsNumberOfPlayers", 8]],
            ["VAR", "$HEALTH7", ["MULTIPLY", "$GAME.arkham.scenarioWarOfTheOuterGodsNumberOfPlayers", 7]],
            ["DO_LOAD_CARDS_SCN", "War of the Outer Gods"],
            ["DO_SET_SPAWN_LOCATION", "86024", 7, 33], //Hub Dimension: Gateway to Destruction
            ["VAR", "$SELECTED_CARDS", ["FILTER_CARDS", "$CARD", ["AND",
                ["EQUAL", "$CARD.sides.A.type", "Agenda"],
                ["EQUAL", "$CARD.sides.A.doom", -2]
            ]]],
            ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                ["SET", "/cardById/$CARD.id/sides/A/doom", "$DOOM"]
            ]],
            ["VAR", "$MAGHAN_ARKAT_ID", ["GET_NAMED_CARD_ID", "Magh'an Ark'at"]],
            ["SET", "/cardById/$MAGHAN_ARKAT_ID/sides/A/health", "$HEALTH8"],
            ["SET", "/cardById/$MAGHAN_ARKAT_ID/sides/A/healthPerInvestigator", 0],
            ["VAR", "$SILENUS_ID", ["GET_NAMED_CARD_ID", "Silenus"]],
            ["SET", "/cardById/$SILENUS_ID/sides/A/health", "$HEALTH8"],
            ["SET", "/cardById/$SILENUS_ID/sides/A/healthPerInvestigator", 0],
            ["VAR", "$EZEL_ZEN_REZL_ID", ["GET_NAMED_CARD_ID", "Ezel-zen-rezl"]],
            ["SET", "/cardById/$EZEL_ZEN_REZL_ID/sides/A/health", "$HEALTH7"],
            ["SET", "/cardById/$EZEL_ZEN_REZL_ID/sides/A/healthPerInvestigator", 0],
            ["DO_SPAWN_MINI_INVESTIGATORS_AT_LOCATION", "Arkham", 5, 3, 3, 0],
            ["DO_SPAWN_ENCOUNTER_AT_LOCATION_SCN", "Disciple of the Swarm", "The Burning Pit", 3, 3, 0],
            ["DO_SPAWN_ENCOUNTER_AT_LOCATION_SCN", "Nihilistic Stargazer", "Athenaeum of the Empty Sky", -3, 3, 0],
            ["DO_SPAWN_ENCOUNTER_AT_LOCATION_SCN", "Zealot of Paradise", "Shrine of Magh'an Ark'at", 3, 3, 0],
            ["SET", "/arkham/ignoreDoomOnPlayerCardsUnlessOneAgenda", true],
            ["SET", "/arkham/placeDoomOnAllAgendas", true],
            ["SET", "/arkham/advanceAllAgendas", true],
            ["SET", "/arkham/finalScenarioMessage", ["FORMAT", "Children of Paradise cards are in Pile C, Death of Stars cards are in Pile D, Swarm of Assimilation cards are in Pile E. Doom thresholds: {0}. Magh'an Ark'at's health: {1}. Silenus' health: {2}. Ezel-zen-rezl's health: {3}. The game will place doom on and advance all agendas, see “Faction Agendas.”", "$DOOM", "$HEALTH8", "$HEALTH8", "$HEALTH7"]],
            ["SCENARIO_INIT_CONTINUE", null]
        ]
    },
    "functions": {
        "WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$ARGS", "$ACTOR_PLAYER", "$SELECTED_VALUE"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK.TARGET_PLAYER"],
                ["VALIDATE_NOT_NULL", "$ARGS", "WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK.ARGS"],
                ["VALIDATE_NOT_EMPTY", "$SELECTED_VALUE", "WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK.SELECTED_VALUE"],
                ["VAR", "$VALUE", ["CALC", "{{$SELECTED_VALUE}}"]],
                ["VALIDATE_GT0", "$VALUE", "WAR_OF_THE_OUTER_GODS_PLAYERS_CALLBACK.SELECTED_VALUE"],
                ["SET", "/arkham/scenarioWarOfTheOuterGodsNumberOfPlayers", "$VALUE"],
                ["LOGF", "{0}{1} set number of players to {2}.", "$ARGS", "$TARGET_PLAYER", "$VALUE"],
                ["ACTION_LIST", "War of the Outer Gods Scenario Question 2"]
            ]
        },
        "WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$ARGS", "$ACTOR_PLAYER", "$SELECTED_VALUE"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK.TARGET_PLAYER"],
                ["VALIDATE_NOT_NULL", "$ARGS", "WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK.ARGS"],
                ["VALIDATE_NOT_EMPTY", "$SELECTED_VALUE", "WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK.SELECTED_VALUE"],
                ["VAR", "$VALUE", ["CALC", "{{$SELECTED_VALUE}}"]],
                ["VALIDATE_GT0", "$VALUE", "WAR_OF_THE_OUTER_GODS_GROUPS_CALLBACK.SELECTED_VALUE"],
                ["SET", "/arkham/scenarioWarOfTheOuterGodsNumberOfGroups", "$VALUE"],
                ["LOGF", "{0}{1} set number of groups to {2}.", "$ARGS", "$TARGET_PLAYER", "$VALUE"],
                ["SCENARIO_INIT_RESUME", "War of the Outer Gods Scenario Init"]
            ]
        }
    }
}