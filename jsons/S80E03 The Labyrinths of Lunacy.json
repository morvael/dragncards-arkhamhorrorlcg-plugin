    "actionLists": {
        "The Labyrinths of Lunacy Scenario Early Init": [
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Decide.", ["LIST", "Play in Epic Multiplayer Mode.", "Play in Single Group Mode."], "The Labyrinths of Lunacy Scenario Question 1"]
        ],
        "The Labyrinths of Lunacy Scenario Question 1": [
            ["LOG", "└── Decision: {{$GAME.arkham.scenarioAnswer1Text}}"],
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Decide.", ["LIST", "Play as Group A.", "Play as Group B.", "Play as Group C."], "The Labyrinths of Lunacy Scenario Question 2"]
        ],
        "The Labyrinths of Lunacy Scenario Question 2": [
            ["LOG", "└── Decision: {{$GAME.arkham.scenarioAnswer2Text}}"],
            ["SCENARIO_INIT_RESUME", "The Labyrinths of Lunacy Scenario Init"]
        ],
        "The Labyrinths of Lunacy Scenario Init": [
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"],
                ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Epic Multiplayer Mode"],
                ["TRUE"],
                ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Single Group Mode"]
            ],
            ["VAR", "$CHAMBERS_OF_SECRETS", ["SHUFFLE_LIST", ["LIST", "70016", "70017", "70018"]]],  //Chamber of Secrets: Bloody Prison, Chamber of Secrets: Mysterious Prison, Chamber of Secrets: Enshrouded Prison
            ["VAR", "$LAST_CHAMBER_OF_SECRETS_ID", ""],
            ["COND",
                ["NOT_EQUAL", "$GAME.arkham.scenarioAnswer2", "A"],
                [
                    ["FOR_EACH_START_STOP_STEP", "$I", 0, 3, 1, [
                        ["DO_CREATE_CARDS_SCN", "Chamber of Secrets", "$CHAMBERS_OF_SECRETS.[$I]", 1, "sharedNotInPlay"],
                        ["UPDATE_VAR", "$LAST_CHAMBER_OF_SECRETS_ID", "$GAME.lastLoadedCardIds.[0]"],
                        ["DO_SET_SPAWN_LOCATION", "$CHAMBERS_OF_SECRETS.[$I]", 55, 7]
                    ]]
                ]
            ],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioAnswer2", "A"],
                    ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Epic Multiplayer Mode - Group A"],
                    ["EQUAL", "$GAME.arkham.scenarioAnswer2", "B"],
                    ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Epic Multiplayer Mode - Group B"],
                    ["TRUE"],
                    ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Epic Multiplayer Mode - Group C"]
                ],
                ["TRUE"],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioAnswer2", "A"],
                    ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Single Group Mode - Group A"],
                    ["EQUAL", "$GAME.arkham.scenarioAnswer2", "B"],
                    ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Single Group Mode - Group B"],
                    ["TRUE"],
                    ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Single Group Mode - Group C"]
                ]
            ],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer2", "A"],
                [
                    ["DO_SPAWN_LOCATION_SCN", "Chamber of Secrets", "$CHAMBERS_OF_SECRETS.[0]", 55, 7],
                    ["DO_SPAWN_MINI_INVESTIGATORS_AT_LOCATION", "Chamber of Secrets", 5, 7, 3, 0],
                    ["COND",
                        ["EQUAL", "$GAME.arkham.scenarioAnswer1", "B"],
                        ["DO_SPAWN_NAMED_AT_LOCATION_SCN", "Key of Mysteries", "Chamber of Secrets", 3, 3, 1]
                    ]
                ],
                ["EQUAL", "$GAME.arkham.scenarioAnswer2", "B"],
                [
                    ["VAR", "$RANDOM_PLAYER", ["RANDOM_INT", 1, "$GAME.numPlayers"]],
                    ["SET", "/playerData/player{{$RANDOM_PLAYER}}/arkham/customSpawnLocation", "Chamber of Rain"],
                    ["DO_SPAWN_MINI_INVESTIGATORS_AT_LOCATION", "Chamber of Sorrows", 5, 7, 3, 0]
                ],
                ["TRUE"],
                [
                    ["DO_SPAWN_MINI_INVESTIGATORS_AT_LOCATION", "Chamber of Night", 5, 7, 3, 0],
                    ["COND",
                        ["EQUAL", "$GAME.arkham.scenarioAnswer1", "B"],
                        ["DO_PUT_CARD_BENEATH", "$GAME.firstPlayer", "$LAST_CHAMBER_OF_SECRETS_ID", ["GET_SCENARIO_CARD_ID"], true, null]
                    ]
                ]
            ],
            ["VAR", "$LABYRINTHINE_HALLS", ["SHUFFLE_LIST", ["LIST", "70025", "70026", "70027"]]],  //Labyrinthine Halls: Foul-smelling Path, Labyrinthine Halls: Corpse-filled Path, Labyrinthine Halls: Overgrown Path
            ["FOR_EACH_START_STOP_STEP", "$I", 0, 3, 1, [
                ["DO_CREATE_CARDS_SCN", "Labyrinthine Halls", "$LABYRINTHINE_HALLS.[$I]", 1, "sharedNotInPlay"]
            ]],
            ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - Remaining Locations"],
            ["DO_SET_SPAWN_LOCATION", "70019", 15, 7],  //Chamber of Rain
            ["DO_SET_SPAWN_LOCATION", "70020", 35, 7],  //Chamber of Sorrows
            ["DO_SET_SPAWN_LOCATION", "70021", 35, 7],  //Chamber of Sorrows
            ["DO_SET_SPAWN_LOCATION", "70022", 75, 55], //Chamber of Night
            ["DO_SET_SPAWN_LOCATION", "70023", 75, 55], //Chamber of Night
            ["DO_SET_SPAWN_LOCATION", "70024", 75, 79], //Chamber of Regret
            ["DO_SET_SPAWN_LOCATION", "70028", 35, 55], //Chamber of Hunger
            ["DO_SET_SPAWN_LOCATION", "70029", 55, 55], //Chamber of Decay
            ["DO_SET_SPAWN_LOCATION", "70030", 15, 55], //Chamber of Rot
            ["DO_SET_SPAWN_LOCATION", "70031", 15, 79], //Chamber of Poison
            ["DO_SET_SPAWN_LOCATION", "70032", 35, 79], //Abandoned Warehouse
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"],
                ["DO_LOAD_CARDS_SCN", "The Labyrinths of Lunacy - The Jailor"]
            ],
            ["DO_CREATE_CARDS_SCN", "Eixodolon's Note", "70039", 1, ["GET_CONTROLLER_PLAY", "$GAME.firstPlayer"]],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer2", "A"],
                ["DO_ADD_CHAOS_TOKENS_SCN", "chaos_elder_thing", 2],
                ["EQUAL", "$GAME.arkham.scenarioAnswer2", "B"],
                ["DO_ADD_CHAOS_TOKENS_SCN", "chaos_tablet", 2],
                ["TRUE"],
                ["DO_ADD_CHAOS_TOKENS_SCN", "chaos_cultist", 2]
            ],
            ["SCENARIO_INIT_CONTINUE", null]
        ]
    }
}