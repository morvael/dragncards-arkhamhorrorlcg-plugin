    "functions": {
        "FORTUNE_AND_FOLLY_PT_2_SHOW_ROLE_CHOICE": {
            "args": ["$TARGET_PLAYER"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "FORTUNE_AND_FOLLY_PT_2_SHOW_ROLE_CHOICE.TARGET_PLAYER"],
                ["PROMPT", "$TARGET_PLAYER", "fortuneAndFollyPt2ChooseRole"],
                ["COND",
                    ["EQUAL", ["GET_NAMED_CARD_ID", "88028"], null], //The Face
                    [
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Face (Practiced)", "F", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Face", "88028", "Practiced"]],
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Face (Unpracticed)", "A", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Face", "88028", "Unpracticed"]]
                    ]
                ],
                ["COND",
                    ["EQUAL", ["GET_NAMED_CARD_ID", "88029"], null], //The Muscle
                    [
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Muscle (Practiced)", "M", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Muscle", "88029", "Practiced"]],
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Muscle (Unpracticed)", "U", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Muscle", "88029", "Unpracticed"]]
                    ]
                ],
                ["COND",
                    ["EQUAL", ["GET_NAMED_CARD_ID", "88030"], null], //The Thief
                    [
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Thief (Practiced)", "T", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Thief", "88030", "Practiced"]],
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Thief (Unpracticed)", "H", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Thief", "88030", "Unpracticed"]]
                    ]
                ],
                ["COND",
                    ["EQUAL", ["GET_NAMED_CARD_ID", "88031"], null], //The Grifter
                    [
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Grifter (Practiced)", "G", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Grifter", "88031", "Practiced"]],
                        ["PROMPT_ADD_OPTION", "$TARGET_PLAYER", "The Grifter (Unpracticed)", "R", ["LIST", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE", "{{$TARGET_PLAYER}}", "The Grifter", "88031", "Unpracticed"]]
                    ]
                ]
            ]
        },
        "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE": {
            "args": ["$TARGET_PLAYER", "$CHOICE_NAME", "$CHOICE_DATABASE_ID", "$PRACTICED"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE.TARGET_PLAYER"],
                ["VALIDATE_NOT_EMPTY", "$CHOICE_NAME", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE.CHOICE_NAME"],
                ["VALIDATE_NOT_EMPTY", "$CHOICE_DATABASE_ID", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE.CHOICE_DATABASE_ID"],
                ["VALIDATE_NOT_EMPTY", "$PRACTICED", "FORTUNE_AND_FOLLY_PT_2_ROLE_CHOICE_DONE.PRACTICED"],
                ["LOGF", "{0} picks {1} ({2}) role.", "$TARGET_PLAYER", "$CHOICE_NAME", "$PRACTICED"],
                ["DO_CREATE_CARDS", "$TARGET_PLAYER", "$CHOICE_NAME", "$CHOICE_DATABASE_ID", 1, ["GET_CONTROLLER_PLAY", "$TARGET_PLAYER"], true, null],
                ["COND",
                    ["EQUAL", "$PRACTICED", "Practiced"],
                    ["DO_FLIP_FACEDOWN", "$TARGET_PLAYER", "$GAME.lastLoadedCardIds.[0]", false, null]
                ],
                ["INCREASE_VAL", "/playerData/$TARGET_PLAYER/arkham/scenarioFortuneAndFollyPt2RolesTaken", 1],
                ["VAR", "$NEXT_PLAYER", ["NEXT_PLAYER", "$TARGET_PLAYER"]],
                ["COND",
                    ["AND",
                        ["EQUAL", "$NEXT_PLAYER", "$GAME.firstPlayer"],
                        ["OR",
                            ["GREATER_THAN", "$GAME.arkham.numPlayers", 1],
                            ["GREATER_EQUAL", "$GAME.playerData.$NEXT_PLAYER.arkham.scenarioFortuneAndFollyPt2RolesTaken", 2]
                        ]
                    ],
                    ["ACTION_LIST", "Fortune and Folly Pt 2 Scenario Init Continue 1"],
                    ["TRUE"],
                    ["FORTUNE_AND_FOLLY_PT_2_SHOW_ROLE_CHOICE", "$NEXT_PLAYER"]
                ]
            ]
        },
        "FORTUNE_AND_FOLLY_PT_2_CLUES_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$ARGS", "$ACTOR_PLAYER", "$SELECTED_VALUE"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "FORTUNE_AND_FOLLY_PT_2_CLUES_CALLBACK.TARGET_PLAYER"],
                ["VALIDATE_NOT_NULL", "$ARGS", "FORTUNE_AND_FOLLY_PT_2_CLUES_CALLBACK.ARGS"],
                ["VALIDATE_NOT_EMPTY", "$SELECTED_VALUE", "FORTUNE_AND_FOLLY_PT_2_CLUES_CALLBACK.SELECTED_VALUE"],
                ["VAR", "$VALUE", ["CALC", "{{$SELECTED_VALUE}}"]],
                ["COND",
                    ["GREATER_THAN", "$VALUE", 0],
                    ["DO_PLACE_CLUES_ON_CARD", "$GAME.firstPlayer", ["GET_NAMED_CARD_ID", "88045"], "$VALUE", true, null] //The Wellspring of Fortune: Symbol of Providence
                ],
                ["ACTION_LIST", "Fortune and Folly Pt 2 Scenario Init Continue 2"]
            ]
        },
        "FORTUNE_AND_FOLLY_PT_2_ALARM_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$ARGS", "$ACTOR_PLAYER", "$SELECTED_VALUE"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "FORTUNE_AND_FOLLY_PT_2_ALARM_CALLBACK.TARGET_PLAYER"],
                ["VALIDATE_NOT_NULL", "$ARGS", "FORTUNE_AND_FOLLY_PT_2_ALARM_CALLBACK.ARGS"],
                ["VALIDATE_NOT_EMPTY", "$SELECTED_VALUE", "FORTUNE_AND_FOLLY_PT_2_ALARM_CALLBACK.SELECTED_VALUE"],
                ["VAR", "$VALUE", ["CALC", "{{$SELECTED_VALUE}}"]],
                ["VALIDATE_GT0", "$VALUE", "FORTUNE_AND_FOLLY_PT_2_ALARM_CALLBACK.SELECTED_VALUE"],
                ["VAR", "$INVESTIGATOR", ["GET_INVESTIGATOR", "$TARGET_PLAYER"]],
                ["DO_PLACE_DOOM_ON_CARD", "$TARGET_PLAYER", "$INVESTIGATOR.id", "$VALUE", true, null],
                ["VAR", "$NEXT_PLAYER", ["NEXT_PLAYER", "$TARGET_PLAYER"]],
                ["COND",
                    ["EQUAL", "$NEXT_PLAYER", "$GAME.firstPlayer"],
                    ["ACTION_LIST", "Fortune and Folly Pt 2 Scenario Init Continue 3"],
                    ["TRUE"],
                    ["CALCULATOR_PROMPT", "$NEXT_PLAYER", "Enter your alarm level", 1, 10, 0, "FORTUNE_AND_FOLLY_PT_2_ALARM_CALLBACK", "$NEXT_PLAYER", "", ""]
                ]
            ]
        }
