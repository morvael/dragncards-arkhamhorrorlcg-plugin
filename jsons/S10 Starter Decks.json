    "automation": {
        "cards": {
            "10079": { //Bewitching
                "rules": {
                    "10079a": { //(reaction) Before you draw your opening hand: Search your deck for up to 3 different Trick cards, and attach them facedown to Bewitching. Shuffle your deck.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/playerData/*/arkham/playerEventBeforeDraw"],
                        "condition": ["TRIGGER_ON_PLAYER_EVENT", "$TARGET.id", "$THIS", "BeforeDraw"],
                        "then": [
                            ["CONSUME_PLAYER_EVENT", "$TARGET.id", "$THIS", "BeforeDraw"],
                            ["DO_FSYSLOG", "{0:C}: {1:P} searches their deck for up to 3 different Trick cards, and attaches them facedown to Bewitching. Shuffles their deck.", ["LIST", "$THIS", "$THIS.controller"]],
                            ["VAR", "$TARGET_PLAYER", "$THIS.controller"],
                            ["VAR", "$DECK_GROUP_ID", ["GET_CONTROLLER_DECK", "$TARGET_PLAYER"]],
                            ["DO_SHOW_SELECTION", "$TARGET_PLAYER", "$DECK_GROUP_ID",
                                ["GET_FILTERED_CARDS", "$DECK_GROUP_ID", null, null, null, ["LIST", "Trick."], null],
                                "$PLAYER_DATA.$TARGET_PLAYER.arkham.deckAttachments.10079",
                                "Select between {0} and {1} different Trick cards to attach them facedown to Bewitching.",
                                0, 3, true, "BEWITCHING_SELECTION_CALLBACK", "$THIS.id"]
                        ]
                    }
                }
            },
            "10109": { //Pelt Shipment
                "rules": {
                    "10109a": { //While Pelt Shipment is in your hand, your maximum hand size is reduced by 3.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inHand", "/cardById/$THIS_ID/controller"],
                        "condition": ["CAN_CHANGE_CONTROLLER_BONUS", "$THIS", "maxHandSize", -3, "$THIS.inHand"],
                        "then": [
                            ["DO_CHANGE_CONTROLLER_BONUS", "$THIS", "maxHandSize", -3, "$THIS.inHand", "{2:P}'s maximum hand size is reduced by 3.", null]
                        ]
                    }
                }
            }
        }
    },
    "functions": {
        "BEWITCHING_SELECTION_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$SOURCE_CARD_ID", "$SELECTED_CARDS"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "BEWITCHING_SELECTION_CALLBACK.TARGET_PLAYER"],
                ["VAR", "$SOURCE_CARD", ["GET_CARD_AND_VALIDATE", "$SOURCE_CARD_ID", "BEWITCHING_SELECTION_CALLBACK.SOURCE_CARD_ID"]],
                ["VALIDATE_NOT_NULL", "$SELECTED_CARDS", "BEWITCHING_SELECTION_CALLBACK.SELECTED_CARDS"],
                ["DO_FSYSLOG", "{0:P} attaches {1} Trick card{1:S} facedown to Bewitching.", ["LIST", "$TARGET_PLAYER", ["LENGTH", "$SELECTED_CARDS"]]],
                ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                    ["DO_ATTACH_CARD_FACEDOWN", "$TARGET_PLAYER", "$CARD.id", "$SOURCE_CARD.id", false, null],
                    ["DO_PEEK_CARD", "$TARGET_PLAYER", "$CARD.id", false, null]
                ]],
                ["PLAYER_EVENT_LOOP", "$TARGET_PLAYER", "BeforeDraw"]
            ]
        }
    }
}