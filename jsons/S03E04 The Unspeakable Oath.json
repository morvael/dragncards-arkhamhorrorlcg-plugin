            ],
            ["SET", "/arkhamFinalScenarioMessage", "Monster enemy cards are in Deck A, Possessed (Lunatic) enemy cards are in Deck B, Patient Confinement cards are in Deck C."],
            ["SCENARIO_INIT_CONTINUE", null]
        ]
