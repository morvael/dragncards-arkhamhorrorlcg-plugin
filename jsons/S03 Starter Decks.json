    "automation": {
        "cards": {
            "03009": {
                "_comment": "Mark Harrigan & Sophie",
                "rules": [
                    {
                        "_comment": "Put Sophie into play",
                        "type": "trigger",
                        "listenTo": ["/playerData/*/arkhamEventPutCardsIntoPlay"],
                        "condition": [
                            ["AND",
                                ["EQUAL", "$TARGET.id", "$THIS.controller"],
                                ["GREATER_THAN", "$TARGET.arkhamEventPutCardsIntoPlay", 0],
                                ["EQUAL", "$THIS.groupId", ["GET_CONTROLLER_DECK", "$THIS.controller"]]
                            ]
                        ],
                        "then": [
                            ["MOVE_CARD", "$THIS.id", ["GET_CONTROLLER_PLAY", "$THIS.controller"], -1]
                        ]
                    }
                ]
            },
            "03003": {
                "_comment": "Sefina Rousseau",
                "rules": [
                    {
                        "_comment": "Set custom draw mode flag",
                        "type": "trigger",
                        "listenTo": ["/playerData/*/arkhamEventPutCardsIntoPlay"],
                        "condition": [
                            ["AND",
                                ["EQUAL", "$TARGET.id", "$THIS.controller"],
                                ["GREATER_THAN", "$TARGET.arkhamEventPutCardsIntoPlay", 0],
                                ["EQUAL", "$THIS.groupId", ["GET_CONTROLLER_INVESTIGATOR", "$THIS.controller"]]
                            ]
                        ],
                        "then": [
                            ["SET", "/playerData/$THIS.controller/arkhamCustomDraw", true]
                        ]
                    },
                    {
                        "_comment": "Perform custom draw",
                        "type": "trigger",
                        "listenTo": ["/playerData/*/arkhamEventCustomDraw"],
                        "condition": [
                            ["AND",
                                ["EQUAL", "$TARGET.id", "$THIS.controller"],
                                ["GREATER_THAN", "$TARGET.arkhamEventCustomDraw", 0],
                                ["EQUAL", "$THIS.groupId", ["GET_CONTROLLER_INVESTIGATOR", "$THIS.controller"]]
                            ]
                        ],
                        "then": [
                            ["VAR", "$STARTING_CARDS", ["GET_STARTING_CARDS", "$THIS.controller", 13]],
                            ["COND",
                                ["AND",
                                    ["GREATER_THAN", "$STARTING_CARDS", 0],
                                    ["EQUAL", ["GET_ALL_CARDS_IN_GROUP_COUNT", ["GET_CONTROLLER_HAND", "$THIS.controller"]], 0]
                                ],
                                ["DO_DRAW_CARDS_EXT", "$THIS.controller", ["GET_CONTROLLER_DECK", "$THIS.controller"], "$STARTING_CARDS", true, true, null]
                            ],
                            ["PROMPT", "$THIS.controller", "decideAboutSefinaHand1"]
                        ]
                    }
                ]
            },
            "03004": {
                "_comment": "Akachi Onyele",
                "rules": [
                    {
                        "_comment": "Set Uses (charges) bonus",
                        "type": "trigger",
                        "listenTo": ["/playerData/*/arkhamEventPutCardsIntoPlay"],
                        "condition": [
                            ["AND",
                                ["EQUAL", "$TARGET.id", "$THIS.controller"],
                                ["GREATER_THAN", "$TARGET.arkhamEventPutCardsIntoPlay", 0],
                                ["EQUAL", "$THIS.groupId", ["GET_CONTROLLER_INVESTIGATOR", "$THIS.controller"]]
                            ]
                        ],
                        "then": [
                            ["INCREASE_VAL", "/playerData/$THIS.controller/arkhamBonusCharges", 1]
                        ]
                    }
                ]
            },
            "03006": {
                "_comment": "Lola Hayes",
                "rules": [
                    {
                        "_comment": "Create Lola Hayes Role card and set custom spawn location if needed",
                        "type": "trigger",
                        "listenTo": ["/playerData/*/arkhamEventPutCardsIntoPlay"],
                        "condition": [
                            ["AND",
                                ["EQUAL", "$TARGET.id", "$THIS.controller"],
                                ["GREATER_THAN", "$TARGET.arkhamEventPutCardsIntoPlay", 0],
                                ["EQUAL", "$THIS.groupId", ["GET_CONTROLLER_INVESTIGATOR", "$THIS.controller"]]
                            ]
                        ],
                        "then": [
                            ["DO_CREATE_CARDS", "$THIS.controller", "Lola Hayes Role card", "r03006", 1, ["GET_CONTROLLER_PLAY", "$THIS.controller"], false, null],
                            ["SET", "/cardById/{{$GAME.lastLoadedCardIds.[0]}}/currentSide", "F"],
                            ["COND",
                                ["OR",
                                    ["AND",
                                        ["EQUAL", "$GAME.arkhamCampaignName", "The Path to Carcosa"],
                                        ["EQUAL", "$GAME.arkhamScenarioName", "Curtain Call"]
                                    ],
                                    ["AND",
                                        ["EQUAL", "$GAME.arkhamCampaignName", "Return to The Path to Carcosa"],
                                        ["EQUAL", "$GAME.arkhamScenarioName", "Return to Curtain Call"]
                                    ]
                                ],
                                ["SET", "/playerData/$THIS.controller/arkhamCustomSpawnLocation", "Backstage"]
                            ]
                        ]
                    }
                ]
            }
        }
    },
