    "functions": {
        "SEFINA_ROUSSEAU_SELECTION_CALLBACK_1": {
            "args": ["$TARGET_PLAYER", "$SOURCE_CARD_ID", "$SELECTED_CARDS"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "SEFINA_ROUSSEAU_SELECTION_CALLBACK_1.TARGET_PLAYER"],
                ["VAR", "$SOURCE_CARD", ["GET_CARD_AND_VALIDATE", "$SOURCE_CARD_ID", "SEFINA_ROUSSEAU_SELECTION_CALLBACK_1.SOURCE_CARD_ID"]],
                ["VALIDATE_NOT_NULL", "$SELECTED_CARDS", "SEFINA_ROUSSEAU_SELECTION_CALLBACK_1.SELECTED_CARDS"],
                ["DO_FSYSLOG", "{0:P} places {1} event{1:S} beneath Sefina Rousseau.", ["LIST", "$TARGET_PLAYER", ["LENGTH", "$SELECTED_CARDS"]]],
                ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                    ["DO_PUT_CARD_BENEATH", "$TARGET_PLAYER", "$CARD.id", "$SOURCE_CARD.id", false, null]
                ]],
                ["VAR", "$HAND_GROUP_ID", ["GET_CONTROLLER_HAND", "$TARGET_PLAYER"]],
                ["VAR", "$HAND_CARDS", ["GET_ALL_CARDS_IN_GROUP", "$HAND_GROUP_ID"]],
                ["VAR", "$HAND_COUNT", ["LENGTH", "$HAND_CARDS"]],
                ["VAR", "$CARDS_KEPT", ["GET_STARTING_CARDS", "$TARGET_PLAYER", 8]],
                ["COND",
                    ["GREATER_THAN", "$HAND_COUNT", "$CARDS_KEPT"],
                    [
                        ["VAR", "$CARDS_TO_DISCARD", ["SUBTRACT", "$HAND_COUNT", "$CARDS_KEPT"]],
                        ["DO_SHOW_SELECTION", "$TARGET_PLAYER", "$HAND_GROUP_ID",
                            "$HAND_CARDS",
                            null,
                            "Select {0} excess card{0:S} to discard from hand.",
                            "$CARDS_TO_DISCARD", "$CARDS_TO_DISCARD", false, "SEFINA_ROUSSEAU_SELECTION_CALLBACK_2", "$SOURCE_CARD_ID"]
                    ],
                    ["TRUE"],
                    ["PLAYER_EVENT_LOOP", "$TARGET_PLAYER", "Draw"]
                ]
            ]
        },
        "SEFINA_ROUSSEAU_SELECTION_CALLBACK_2": {
            "args": ["$TARGET_PLAYER", "$SOURCE_CARD_ID", "$SELECTED_CARDS"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "SEFINA_ROUSSEAU_SELECTION_CALLBACK_2.TARGET_PLAYER"],
                ["VAR", "$SOURCE_CARD", ["GET_CARD_AND_VALIDATE", "$SOURCE_CARD_ID", "SEFINA_ROUSSEAU_SELECTION_CALLBACK_2.SOURCE_CARD_ID"]],
                ["VALIDATE_NOT_NULL", "$SELECTED_CARDS", "SEFINA_ROUSSEAU_SELECTION_CALLBACK_2.SELECTED_CARDS"],
                ["DO_FSYSLOG", "{0:P} discards {1} excess card{1:S} from their hand.", ["LIST", "$TARGET_PLAYER", ["LENGTH", "$SELECTED_CARDS"]]],
                ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                    ["DO_DISCARD_CARD", "$TARGET_PLAYER", "$CARD.id", false, null]
                ]],
                ["PLAYER_EVENT_LOOP", "$TARGET_PLAYER", "Draw"]
            ]
        },
        "CAN_SWITCH_ROLE": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$NEW_SIDE", "$ACTOR_PLAYER"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "CAN_SWITCH_ROLE.TARGET_PLAYER"],
                ["VAR", "$TARGET_CARD", ["GET_CARD_AND_VALIDATE", "$TARGET_CARD_ID", "CAN_SWITCH_ROLE.TARGET_CARD_ID"]],
                ["VALIDATE_NOT_EMPTY", "$NEW_SIDE", "CAN_SWITCH_ROLE.NEW_SIDE"],
                ["AND",
                    ["EQUAL", "$TARGET_CARD.currentFace.type", "Role"],
                    ["IN_STRING", "$TARGET_CARD.groupId", "Play"],
                    ["IN_STRING", "ABCDEF", "$NEW_SIDE"],
                    ["NOT_EQUAL", "$TARGET_CARD.currentSide", "$NEW_SIDE"]
                ]
            ]
        },
        "DO_SWITCH_ROLE": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$NEW_SIDE", "$LOG_EVENT", "$ACTOR_PLAYER"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "DO_SWITCH_ROLE.TARGET_PLAYER"],
                ["VAR", "$TARGET_CARD", ["GET_CARD_AND_VALIDATE", "$TARGET_CARD_ID", "DO_SWITCH_ROLE.TARGET_CARD_ID"]],
                ["VALIDATE_NOT_EMPTY", "$NEW_SIDE", "DO_SWITCH_ROLE.NEW_SIDE"],
                ["VALIDATE_BOOLEAN", "$LOG_EVENT", "DO_SWITCH_ROLE.LOG_EVENT"],
                ["COND",
                    ["CAN_SWITCH_ROLE", "$TARGET_PLAYER", "$TARGET_CARD_ID", "$NEW_SIDE", "$ACTOR_PLAYER"],
                    [
                        ["DO_LOG", "$LOG_EVENT", "$ACTOR_PLAYER", "$TARGET_PLAYER", "change Lola to the {{$TARGET_CARD.sides.$NEW_SIDE.name}}.", "changes Lola to the {{$TARGET_CARD.sides.$NEW_SIDE.name}}."],
                        ["SET", "/cardById/$TARGET_CARD_ID/currentSide", "$NEW_SIDE"]
                    ]
                ]
            ]
        },
        "STICK_TO_THE_PLAN_SELECTION_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$SOURCE_CARD_ID", "$SELECTED_CARDS"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "STICK_TO_THE_PLAN_SELECTION_CALLBACK.TARGET_PLAYER"],
                ["VAR", "$SOURCE_CARD", ["GET_CARD_AND_VALIDATE", "$SOURCE_CARD_ID", "STICK_TO_THE_PLAN_SELECTION_CALLBACK.SOURCE_CARD_ID"]],
                ["VALIDATE_NOT_NULL", "$SELECTED_CARDS", "STICK_TO_THE_PLAN_SELECTION_CALLBACK.SELECTED_CARDS"],
                ["DO_FSYSLOG", "{0:P} attaches {1} Tactic and/or Supply event{1:S} facedown to Stick to the Plan.", ["LIST", "$TARGET_PLAYER", ["LENGTH", "$SELECTED_CARDS"]]],
                ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                    ["DO_ATTACH_CARD_FACEDOWN", "$TARGET_PLAYER", "$CARD.id", "$SOURCE_CARD.id", false, null],
                    ["DO_PEEK_CARD", "$TARGET_PLAYER", "$CARD.id", false, null]
                ]],
                ["PLAYER_EVENT_LOOP", "$TARGET_PLAYER", "BeforeDraw"]
            ]
        }
    },
