    "automation": {
        "cards": {
            "03001": { //Mark Harrigan: The Soldier
                "rules": {
                    "03001a": { //You begin the game with Sophie (In Loving Memory) in play.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay"],
                        "condition": ["TRIGGER_IN_PLAY", "$THIS"],
                        "then": [
                            ["CARD_ABILITY_ON", "$THIS"],
                            ["DO_FSYSLOG", "{0:C}: {1:P} begins the game with Sophie: In Loving Memory in play.", ["LIST", "$THIS", "$THIS.controller"]],
                            ["DO_BEGIN_WITH_CARD_IN_PLAY", "$THIS.controller", "Sophie"]
                        ]
                    }
                }
            },
            "03003": { //Sefina Rousseau: The Painter
                "rules": {
                    "03003a": { //Forced - When you would draw your opening hand: Draw 13 cards, instead. Choose up to 5 events to place beneath this card and keep 8 cards as your opening hand. Discard the rest. (You cannot mulligan.)
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/playerData/*/arkham/playerEventDraw"],
                        "condition": ["TRIGGER_ON_PLAYER_EVENT", "$TARGET.id", "$THIS", "Draw"],
                        "then": [
                            ["CANCEL_PLAYER_EVENT", "$TARGET.id", "$THIS", "Draw"],
                            ["DO_FSYSLOG", "{0:C}: {1:P} draws 13 cards instead of drawing their opening hand. Chooses up to 5 events to place beneath Sefina Rousseau and keeps 8 cards as their opening hand. Discards the rest. (They cannot mulligan.)", ["LIST", "$THIS", "$THIS.controller"]],
                            ["VAR", "$TARGET_PLAYER", "$THIS.controller"],
                            ["VAR", "$DECK_GROUP_ID", ["GET_CONTROLLER_DECK", "$TARGET_PLAYER"]],
                            ["VAR", "$HAND_GROUP_ID", ["GET_CONTROLLER_HAND", "$TARGET_PLAYER"]],
                            ["VAR", "$STARTING_CARDS", ["GET_STARTING_CARDS", "$TARGET_PLAYER", 13]],
                            ["COND",
                                ["GREATER_THAN", "$STARTING_CARDS", 0],
                                ["DO_DRAW_CARDS_EXT", "$TARGET_PLAYER", "$DECK_GROUP_ID", "$STARTING_CARDS", true, true, null]
                            ],
                            ["DO_SHOW_SELECTION", "$TARGET_PLAYER", "$HAND_GROUP_ID",
                                ["GET_FILTERED_CARDS", "$HAND_GROUP_ID", null, ["LIST", "Event"], null, null, ["LIST", "The Painted World"]],
                                null,
                                "Select between {0} and {1} events to place beneath Sefina Rousseau.",
                                0, 5, false, "SEFINA_ROUSSEAU_SELECTION_CALLBACK_1", "$THIS.id"]
                        ]
                    }
                }
            },
            "03004": { //Akachi Onyele: The Shaman
                "rules": {
                    "03004a": { //Your assets with "uses (charges)" enter play with 1 additional charge on them.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay", "/cardById/$THIS_ID/controller"],
                        "condition": ["CAN_CHANGE_CONTROLLER_BONUS", "$THIS", "bonusCharges", 1, "$THIS.inPlay"],
                        "then": [
                            ["DO_CHANGE_CONTROLLER_BONUS", "$THIS", "bonusCharges", 1, "$THIS.inPlay", "{2:P}'s assets with “uses (charges)” enter play with 1 additional charge on them.", null]
                        ]
                    }
                }
            },
            "03006": { //Lola Hayes: The Actress
                "rules": {
                    "03006a": { //Forced - After you draw your opening hand: Choose a role (Survivor, Guardian, Seeker, Rogue, Mystic, or Neutral). You can only play, commit, or trigger abilities on Neutral cards or cards of your role.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay"],
                        "condition": ["TRIGGER_IN_PLAY", "$THIS"],
                        "then": [
                            ["CARD_ABILITY_ON", "$THIS"],
                            ["DO_CREATE_CARDS", "$THIS.controller", "Lola Hayes Role card", "r03006", 1, ["GET_CONTROLLER_PLAY", "$THIS.controller"], false, null],
                            ["SET", "/cardById/{{$GAME.lastLoadedCardIds.[0]}}/currentSide", "F"]
                        ]
                    },
                    "03006b": { //Forced - After you draw your opening hand: Choose a role (Survivor, Guardian, Seeker, Rogue, Mystic, or Neutral). You can only play, commit, or trigger abilities on Neutral cards or cards of your role.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/playerData/*/arkham/playerEventAfterDraw"],
                        "condition": ["TRIGGER_ON_PLAYER_EVENT", "$TARGET.id", "$THIS", "AfterDraw", 1],
                        "then": [
                            ["CONSUME_PLAYER_EVENT", "$TARGET.id", "$THIS", "AfterDraw", 1],
                            ["DO_FSYSLOG", "{0:C}: {1:P} chooses a role (Survivor, Guardian, Seeker, Rogue, Mystic, or Neutral). They can only play, commit, or trigger abilities on Neutral cards or cards of their role.", ["LIST", "$THIS", "$THIS.controller"]],
                            ["PLAYER_EVENT_LOOP", "$TARGET.id", "AfterDraw"]
                        ]
                    }
                }
            },
