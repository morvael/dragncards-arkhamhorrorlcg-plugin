        },
        "decideAboutSefinaHand2": {
            "args": ["$CARDS_TO_DISCARD"],
            "message": "You should discard {{$CARDS_TO_DISCARD}} excess card(s) from hand. Target cards to discard.",
            "options": [
                {
                    "label": "Proceed.",
                    "hotkey": "P",
                    "code": [
                        ["DO_DISCARD_EXCESS_SEFINA_CARDS", "$PLAYER_N", "$CARDS_TO_DISCARD"]
                    ]
                }
            ]
        }
    },
