                "showIf": ["AND",
                    ["EQUAL", "$GAME.arkhamScenarioInit", false],
                    ["EQUAL", "$ACTIVE_FACE.type", "Investigator"],
                    ["NOT_EQUAL", "$ACTIVE_CARD.eliminated", true]
                ]
            },
            {
