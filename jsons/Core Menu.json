                                ["EQUAL", "$ACTIVE_CARD.sides.A.type", "Story"],
                                ["EQUAL", "$ACTIVE_CARD.sides.A.type", "Key"],
                                ["AND",
                                    ["EQUAL", "$ACTIVE_CARD.sides.A.type", "Investigator"],
                                    ["EQUAL", "$ACTIVE_CARD.eliminated", true]
                                ],
                                ["AND",
                                    ["EQUAL", "$ACTIVE_CARD.sides.A.type", "MiniInvestigator"],
                                    ["EQUAL", "$ACTIVE_CARD.eliminated", true]
                                ]
                            ]
                        ]
                    ]
                ]
