    "defaultActions": [
    ],
    "hotkeys": {
        "token": [
            {"key": "1", "tokenType": "resource", "label": "icon(https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/resource.webp)"},
            {"key": "2", "tokenType": "clue", "label": "icon(https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/clue.webp)"},
            {"key": "3", "tokenType": "doom", "label": "icon(https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/doom.webp)"},
            {"key": "4", "tokenType": "damage", "label": "icon(https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/damage.webp)"},
            {"key": "5", "tokenType": "horror", "label": "icon(https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/horror.webp)"},
            {"key": "6", "tokenType": "freeze", "label": "icon(https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/time.webp)"}
        ],
        "game": [
            {"key": "<", "actionList": "retractAction", "label": "id:retractAction"},
            {"key": ",", "actionList": "retractAction", "label": "id:retractAction"},
            {"key": ">", "actionList": "takeAction", "label": "id:takeAction"},
            {"key": ".", "actionList": "takeAction", "label": "id:takeAction"},
            {"key": "-", "actionList": "spendResources", "label": "id:spendResources"},
            {"key": "=", "actionList": "gainResources", "label": "id:gainResources"},
            {"key": "+", "actionList": "gainResources", "label": "id:gainResources"},
            {"key": "B", "actionList": "toggleChaosBag", "label": "id:toggleChaosBag"},
            {"key": "C", "actionList": "revealChaosToken", "label": "id:revealChaosToken"},
            {"key": "H", "actionList": "nameChaosToken", "label": "id:nameChaosToken"},
            {"key": "K", "actionList": "toggleTokens", "label": "id:toggleTokens"},
            {"key": "M", "actionList": "toggleOffMap", "label": "id:toggleOffMap"},
            {"key": "N", "actionList": "toggleNotInPlay", "label": "id:toggleNotInPlay"},
            {"key": "O", "actionList": "toggleSideboard", "label": "id:toggleSideboard"},
            {"key": "Q", "actionList": "drawEncounterCard", "label": "id:drawEncounterCard"},
            {"key": "U", "actionList": "addCurseToken", "label": "id:addCurseToken"},
            {"key": "Shift+U", "actionList": "removeCurseToken", "label": "id:removeCurseToken"},
            {"key": "V", "actionList": "toggleVictoryDisplay", "label": "id:toggleVictoryDisplay"},
            {"key": "W", "actionList": "drawPlayerCard", "label": "id:drawPlayerCard"},
            {"key": "Shift+X", "actionList": "discardVoid", "label": "id:discardVoid"},
            {"key": "Y", "actionList": "addBlessToken", "label": "id:addBlessToken"},
            {"key": "Shift+Y", "actionList": "removeBlessToken", "label": "id:removeBlessToken"},
            {"key": "Ctrl+ArrowUp", "actionList": "moveAllCardsUp", "label": "id:moveAllCardsUp"},
            {"key": "Ctrl+ArrowDown", "actionList": "moveAllCardsDown", "label": "id:moveAllCardsDown"},
            {"key": "Ctrl+ArrowLeft", "actionList": "moveAllCardsLeft", "label": "id:moveAllCardsLeft"},
            {"key": "Ctrl+ArrowRight", "actionList": "moveAllCardsRight", "label": "id:moveAllCardsRight"}
        ],
        "card": [
            {"key": "7", "actionList": "calculateWillpower", "label": "id:calculateWillpower"},
            {"key": "8", "actionList": "calculateIntellect", "label": "id:calculateIntellect"},
            {"key": "9", "actionList": "calculateCombat", "label": "id:calculateCombat"},
            {"key": "0", "actionList": "calculateAgility", "label": "id:calculateAgility"},
            {"key": "/", "actionList": "peekCard", "label": "id:peekCard"},
            {"key": "A", "actionList": "attachCard", "label": "id:attachCard"},
            {"key": "Ctrl+A", "actionList": "attachCardFacedown", "label": "id:attachCardFacedown"},
            {"key": "D", "actionList": "detachCard", "label": "id:detachCard"},
            {"key": "E", "actionList": "exhaustCard", "label": "id:exhaustCard"},
            {"key": "F", "actionList": "flipCard", "label": "id:flipCard"},
            {"key": "G", "actionList": "advanceStoryFromCard", "label": "id:advanceStoryFromCard"},
            {"key": "I", "actionList": "investigatorTurn", "label": "id:investigatorTurn"},
            {"key": "J", "actionList": "commitCard", "label": "id:commitCard"},
            {"key": "L", "actionList": "replaceLocation", "label": "id:replaceLocation"},
            {"key": "P", "actionList": "playCard", "label": "id:playCard"},
            {"key": "Ctrl+P", "actionList": "putCardIntoPlay", "label": "id:putCardIntoPlay"},
            {"key": "R", "actionList": "readyCard", "label": "id:readyCard"},
            {"key": "S", "actionList": "spawnCard", "label": "id:spawnCard"},
            {"key": "X", "actionList": "discardCard", "label": "id:discardCard"},
            {"key": "Z", "actionList": "putCardBeneath", "label": "id:putCardBeneath"},
            {"key": "'", "actionList": "moveClue", "label": "id:moveClue"},
            {"key": "[", "actionList": "sendToBack", "label": "id:sendToBack"},
            {"key": "]", "actionList": "bringToFront", "label": "id:bringToFront"},
            {"key": "Delete", "actionList": "removeCard", "label": "id:removeCard"},
            {"key": "Ctrl+Delete", "actionList": "deleteSupplyCard", "label": "id:deleteSupplyCard"}
        ]
    },
    "imageUrlPrefix": {
        "Default": "https://dragncards-ahlcg.s3.amazonaws.com/images/",
        "English": "https://dragncards-ahlcg.s3.amazonaws.com/images/",
        "Spanish": "https://dragncards-ahlcg.s3.amazonaws.com/images/es/"
    },
    "saveGame": {
        "metadata": {
        }
    },
    "spawnExistingCardModal": {
        "columnProperties": ["name", "faction", "type", "traits", "packName", "numberInPack"],
        "loadGroupIds": ["sharedNotInPlay","sharedLocations","player1Hand","player2Hand","player3Hand","player4Hand","player1Deck","player2Deck","player3Deck","player4Deck"]
    },
    "topBarCounters": {
        "shared": [
            {
                "label": "id:roundCounter",
                "imageUrl": "https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/time.webp",
                "gameProperty": "roundNumber"
            }
        ],
        "player": [
            {
                "label": "id:resourcesCounter",
                "imageUrl": "https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/resource.webp",
                "playerProperty": "resources"
            },
            {
                "label": "id:actionsTakenCounter",
                "imageUrl": "https://dragncards-ahlcg.s3.amazonaws.com/images/tokens/action.webp",
                "playerProperty": "actionsTaken"
            }
        ]
    },
    "labels": {
        "roundCounter": {
            "English": "Round"
        },
        "resourcesCounter": {
            "English": "Resources"
        },
        "actionsTakenCounter": {
            "English": "Actions Taken"
        }
    }
}