        "Treachery":              {"width": 0.69, "height": 1.00, "tokens": ["resource","clue","damage","horror","doom","freeze"]},
        "Event":                  {"width": 0.69, "height": 1.00, "tokens": ["resource","clue","damage","horror","doom","freeze"]},
        "Skill":                  {"width": 0.69, "height": 1.00, "tokens": ["resource","clue","damage","horror","doom","freeze"]},
        "Enemy":                  {"width": 0.69, "height": 1.00, "tokens": ["resource","clue","damage","horror","doom","freeze"]},
        "Scenario":               {"width": 0.69, "height": 1.00, "tokens": ["resource","clue","damage","horror","doom","freeze"]},
        "Reference":              {"width": 0.69, "height": 1.00, "tokens": ["resource","clue","damage","horror","doom","freeze"]},
        "Role":                   {"width": 0.69, "height": 1.00, "tokens": []},
        "Agenda":                 {"width": 1.00, "height": 0.69, "tokens": ["resource","clue","damage","horror","doom"]},
