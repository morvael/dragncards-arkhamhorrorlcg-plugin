    "automation": {
        "cards": {
            "08004": { //Norman Withers: The Astronomer
                "rules": {
                    "08004a": { //Play with the top card of your deck revealed.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay", "/cardById/$THIS_ID/controller"],
                        "condition": ["CAN_CHANGE_CONTROLLER_BONUS", "$THIS", "playWithTopDeckCardRevealed", 1, "$THIS.inPlay"],
                        "then": [
                            ["DO_CHANGE_CONTROLLER_BONUS", "$THIS", "playWithTopDeckCardRevealed", 1, "$THIS.inPlay", "{2:P} plays with the top card of their deck revealed.", null]
                        ]
                    }
                }
            },
            "98007": { //Norman Withers: The Astronomer (promo)
                "inheritFrom": "08004"
            },
            "08031": { //Forced Learning
                "rules": {
                    "08031a": { //During each upkeep phase, instead of drawing 1 card, draw 2 cards and discard 1 of them.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/playerData/*/arkham/playerEventUpkeepDraw"],
                        "condition": ["TRIGGER_ON_PLAYER_EVENT", "$TARGET.id", "$THIS", "UpkeepDraw", 1],
                        "then": [
                            ["CANCEL_PLAYER_EVENT", "$TARGET.id", "$THIS", "UpkeepDraw", 1],
                            ["DO_FSYSLOG", "{0:C}: instead of drawing 1 card, {1:P} draws 2 cards and discard 1 of them.", ["LIST", "$THIS", "$THIS.controller"]],
                            ["VAR", "$TARGET_PLAYER", "$THIS.controller"],
                            ["VAR", "$HAND_GROUP_ID", ["GET_CONTROLLER_HAND", "$TARGET_PLAYER"]],
                            ["VAR", "$OLD_HAND", ["GET_ALL_CARDS_IN_GROUP", "$HAND_GROUP_ID"]],
                            ["DO_DRAW_CARDS", "$TARGET_PLAYER", ["GET_CONTROLLER_DECK", "$TARGET_PLAYER"], 2, true, null],
                            ["VAR", "$NEW_HAND", ["GET_ALL_CARDS_IN_GROUP", "$HAND_GROUP_ID"]],
                            ["VAR", "$NEW_CARDS", ["LIST"]],
                            ["FOR_EACH_VAL", "$NEW_CARD", "$NEW_HAND", [
                                ["VAR", "$FOUND", false],
                                ["FOR_EACH_VAL", "$OLD_CARD", "$OLD_HAND", [
                                    ["COND",
                                        ["EQUAL", "$NEW_CARD.id", "$OLD_CARD.id"],
                                        ["UPDATE_VAR", "$FOUND", true]
                                    ]
                                ]],
                                ["COND",
                                    ["EQUAL", "$FOUND", false],
                                    ["UPDATE_VAR", "$NEW_CARDS", ["APPEND", "$NEW_CARDS", "$NEW_CARD"]]
                                ]
                            ]],
                            ["COND",
                                ["GREATER_THAN", ["LENGTH", "$NEW_CARDS"], 0],
                                [
                                    ["DO_SHOW_SELECTION", "$TARGET_PLAYER", "$HAND_GROUP_ID",
                                        "$NEW_CARDS",
                                        null,
                                        "Target {0} card to discard it.",
                                        1, 1, false, "FORCED_LEARNING_SELECTION_CALLBACK", "$THIS.id"]
                                ],
                                ["TRUE"],
                                ["PLAYER_EVENT_LOOP", "$TARGET.id", "UpkeepDraw"]
                            ]
                        ]
                    }
                }
            }
        }
    },
    "functions": {
        "FORCED_LEARNING_SELECTION_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$SOURCE_CARD_ID", "$SELECTED_CARDS"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "FORCED_LEARNING_SELECTION_CALLBACK.TARGET_PLAYER"],
                ["VAR", "$SOURCE_CARD", ["GET_CARD_AND_VALIDATE", "$SOURCE_CARD_ID", "FORCED_LEARNING_SELECTION_CALLBACK.SOURCE_CARD_ID"]],
                ["VALIDATE_NOT_NULL", "$SELECTED_CARDS", "FORCED_LEARNING_SELECTION_CALLBACK.SELECTED_CARDS"],
                ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                    ["DO_DISCARD_CARD", "$TARGET_PLAYER", "$CARD.id", true, null]
                ]],
                ["PLAYER_EVENT_LOOP", "$TARGET_PLAYER", "UpkeepDraw"]
            ]
        }
    }
