    "automation": {
        "cards": {
            "06004": { //Luke Robinson: The Dreamer
                "rules": {
                    "06004a": { //You begin the game with Gate Box in play.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay"],
                        "condition": ["TRIGGER_IN_PLAY", "$THIS"],
                        "then": [
                            ["CARD_ABILITY_ON", "$THIS"],
                            ["DO_FSYSLOG", "{0:C}: {1:P}, you begin the game with Gate Box: Worlds within Worlds in play.", ["LIST", "$THIS", "$THIS.controller"]],
                            ["DO_BEGIN_WITH_CARD_IN_PLAY", "$THIS.controller", "Gate Box"]
                        ]
                    }
                }
            },
            "06005": { //Patrice Hathaway: The Violinist
                "rules": {
                    "06005a": { //Your maximum hand size is reduced by 3.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay", "/cardById/$THIS_ID/controller"],
                        "condition": ["CAN_CHANGE_CONTROLLER_BONUS", "$THIS", "arkhamMaxHandSize", -3, "$THIS.inPlay"],
                        "then": [
                            ["DO_CHANGE_CONTROLLER_BONUS", "$THIS", "arkhamMaxHandSize", -3, "$THIS.inPlay", "your maximum hand size is reduced by 3.", null]
                        ]
                    },
                    "06005b": { //During each upkeep phase, instead of drawing 1 card, discard all non-weakness cards in your hand and draw until you have 5 cards in hand.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/playerData/*/arkham/playerEventUpkeepDraw"],
                        "condition": ["TRIGGER_ON_PLAYER_EVENT", "$TARGET.id", "$THIS", "UpkeepDraw", 1],
                        "then": [
                            ["CANCEL_PLAYER_EVENT", "$TARGET.id", "$THIS", "UpkeepDraw", 1],
                            ["DO_FSYSLOG", "{0:C}: {1:P}, instead of drawing 1 card, discard all non-weakness cards in your hand and draw until you have 5 cards in hand.", ["LIST", "$THIS", "$THIS.controller"]],
                            ["VAR", "$HAND_ID", ["GET_CONTROLLER_HAND", "$TARGET.id"]],
                            ["VAR", "$SELECTED_CARDS", ["FILTER_CARDS", "$CARD", ["AND",
                                ["EQUAL", "$CARD.groupId", "$HAND_ID"],
                                ["NOT_EQUAL", ["IN_STRING", "$CARD.sides.A.subtype", "Weakness"], true]
                            ]]],
                            ["COND",
                                ["GREATER_THAN", ["LENGTH", "$SELECTED_CARDS", 0], 0],
                                [
                                    ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                                        ["DO_DISCARD_CARD", "$TARGET.id", "$CARD.id", true, null]
                                    ]]
                                ]
                            ],
                            ["VAR", "$CARDS_IN_HAND", ["GET_ALL_CARDS_IN_GROUP_COUNT", "$HAND_ID"]],
                            ["VAR", "$CARDS_TO_DRAW", ["SUBTRACT", 5, "$CARDS_IN_HAND"]],
                            ["COND",
                                ["GREATER_THAN", "$CARDS_TO_DRAW", 0],
                                ["DO_DRAW_CARDS", "$TARGET.id", ["GET_CONTROLLER_DECK", "$TARGET.id"], "$CARDS_TO_DRAW", true, null]
                            ],
                            ["PLAYER_EVENT_LOOP", "$TARGET.id", "UpkeepDraw"]
                        ]
                    }
                }
            },
            "06159": { //Dream-Enhancing Serum
                "rules": {
                    "06159a": { //Only the first copy of each card in your hand counts toward your maximum hand size.
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/inPlay", "/cardById/$THIS_ID/controller"],
                        "condition": ["CAN_CHANGE_CONTROLLER_BONUS", "$THIS", "arkhamHandSizeCountUnique", 1, "$THIS.inPlay"],
                        "then": [
                            ["DO_CHANGE_CONTROLLER_BONUS", "$THIS", "arkhamHandSizeCountUnique", 1, "$THIS.inPlay", "only the first copy of each card in your hand counts toward your maximum hand size.", null]
                        ]
                    }
                }
            }
        }
    }
