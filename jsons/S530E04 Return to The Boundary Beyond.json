        "Return to The Boundary Beyond Scenario Early Init": [
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Check Campaign Log.", ["LIST", "The investigators forged a bond with Ichtaca.", "Ichtaca is in the dark."], "Return to The Boundary Beyond Scenario Question 1"]
        ],
        "Return to The Boundary Beyond Scenario Question 1": [
            ["LOG", "└── Campaign Log: {{$GAME.arkhamScenarioAnswer1Text}}"],
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer1", "B"],
                [
                    ["FOR_EACH_VAL", "$PLAYER_I", "$PLAYER_ORDER", [
                        ["DECREASE_VAL", "/playerData/$PLAYER_I/arkhamStartingCardsMod", 2]
                    ]]
                ]
            ],
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Check Campaign Log.", ["LIST", "The investigators found the missing relic.", "The relic is missing."], "Return to The Boundary Beyond Scenario Question 2"]
        ],
        "Return to The Boundary Beyond Scenario Question 2": [
            ["LOG", "└── Campaign Log: {{$GAME.arkhamScenarioAnswer2Text}}"],
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer2", "A"],
                ["DO_REPLACE_CARD_SCN", "04061", "Relic of Ages: Forestalling the Future", "04191"] //Relic of Ages: …A Device, of Some Sort -> Relic of Ages: Forestalling the Future
            ],
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Check Campaign Log.", ["LIST", "The investigators rescued Alejandro.", "Alejandro is missing."], "Return to The Boundary Beyond Scenario Question 3"]
        ],
        "Return to The Boundary Beyond Scenario Question 3": [
            ["LOG", "└── Campaign Log: {{$GAME.arkhamScenarioAnswer3Text}}"],
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer3", "A"],
                [
                    ["FOR_EACH_VAL", "$PLAYER_I", "$PLAYER_ORDER", [
                        ["INCREASE_VAL", "/playerData/$PLAYER_I/arkhamStartingResourcesMod", 2]
                    ]]
                ]
            ],
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Check supplies.", ["LIST", "No gasoline was crossed off.", "The above is not true."], "Return to The Boundary Beyond Scenario Question 4"]
        ],
        "Return to The Boundary Beyond Scenario Question 4": [
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer4", "A"],
                [
                    ["LOG", "└── Supplies: {{$GAME.arkhamScenarioAnswer4Text}}"],
                    ["FOR_EACH_VAL", "$PLAYER_I", "$PLAYER_ORDER", [
                        ["SET", "/playerData/$PLAYER_I/arkhamDisableMulligan", true]
                    ]]
                ]
            ],
            ["SCENARIO_INIT_RESUME", "Return to The Boundary Beyond Scenario Init"]
        ],
