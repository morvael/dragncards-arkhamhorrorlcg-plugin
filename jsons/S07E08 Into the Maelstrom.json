                ["UPDATE_VAR", "$KEYS", ["APPEND", "$KEYS", "key_blue"]]
            ],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer3", "B"],
                ["UPDATE_VAR", "$KEYS", ["APPEND", "$KEYS", "key_red"]]
            ],
