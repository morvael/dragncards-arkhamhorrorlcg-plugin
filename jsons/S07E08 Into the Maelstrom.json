            ["VAR", "$SPAWN_TOPS", ["LIST", 10, 10, 10, 43, 43, 76, 76, 76]],
            ["FOR_EACH_START_STOP_STEP", "$I", 0, 8, 1, [
                ["DO_SPAWN_LOCATION_SCN", "Tidal Tunnel", "$TIDAL_TUNNELS.[$I]", "$SPAWN_LEFTS.[$I]", "$SPAWN_TOPS.[$I]"]
