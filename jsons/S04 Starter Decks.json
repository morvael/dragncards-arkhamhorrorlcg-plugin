                        "condition": ["TRUE"],
                        "then": [
                            ["SET", "/cardById/$THIS.id/sides/A/skillCombat", "$THIS.tokens.damage"],
                            ["SET", "/cardById/$THIS.id/sides/A/skillAgility", "$THIS.tokens.damage"]
                        ]
                    },
                    "04005b": {
                        "type": "trigger",
                        "priority": 10,
                        "listenTo": ["/cardById/$THIS_ID/tokens/horror"],
