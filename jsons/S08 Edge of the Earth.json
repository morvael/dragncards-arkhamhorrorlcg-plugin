    "functions": {
        //generic function returning status
        "EDGE_OF_THE_EARTH_GET_STATUS": {
            "args": ["$VARIABLE_TEMPLATE", "$DATABASE_ID", "$DEFAULT"],
            "code": [
                ["VALIDATE_NOT_EMPTY", "$VARIABLE_TEMPLATE", "EDGE_OF_THE_EARTH_GET_STATUS.VARIABLE_TEMPLATE"],
                ["VALIDATE_NOT_EMPTY", "$DATABASE_ID", "EDGE_OF_THE_EARTH_GET_STATUS.DATABASE_ID"],
                ["VALIDATE_NOT_EMPTY", "$DEFAULT", "EDGE_OF_THE_EARTH_GET_STATUS.DEFAULT"],
                ["VAR", "$VARIABLE", ["FORMAT", "$VARIABLE_TEMPLATE", "$DATABASE_ID"]],
                ["COND",
                    ["DEFINED", "$GAME.arkham.{{$VARIABLE}}"],
                    "$GAME.arkham.{{$VARIABLE}}",
                    ["TRUE"],
                    "$DEFAULT"
                ]
            ]
        },
        //generic function setting status
        "EDGE_OF_THE_EARTH_SET_STATUS": {
            "args": ["$VARIABLE_TEMPLATE", "$DATABASE_ID", "$STATUS"],
            "code": [
                ["VALIDATE_NOT_EMPTY", "$VARIABLE_TEMPLATE", "EDGE_OF_THE_EARTH_SET_STATUS.VARIABLE_TEMPLATE"],
                ["VALIDATE_NOT_EMPTY", "$DATABASE_ID", "EDGE_OF_THE_EARTH_SET_STATUS.DATABASE_ID"],
                ["VALIDATE_NOT_EMPTY", "$STATUS", "EDGE_OF_THE_EARTH_SET_STATUS.STATUS"],
                ["VAR", "$VARIABLE", ["FORMAT", "$VARIABLE_TEMPLATE", "$DATABASE_ID"]],
                ["SET", "/arkham/{{$VARIABLE}}", "$STATUS"]
            ]
        },
        //generic function returning next element on the list after the given element (or first element for null, or not found)
        "EDGE_OF_THE_EARTH_GET_NEXT_ID": {
            "args": ["$DATABASE_ID", "$LIST"],
            "code": [
                //$DATABASE_ID may be null
                ["VALIDATE_GT0", ["LENGTH", "$LIST"], "EDGE_OF_THE_EARTH_GET_NEXT_ID.LIST"],
                ["VAR", "$COUNT", ["LENGTH", "$LIST"]],
                ["COND",
                    ["EQUAL", "$DATABASE_ID", null],
                    "$LIST.[0]",
                    ["TRUE"],
                    [
                        ["VAR", "$I", ["ADD", ["GET_INDEX", "$LIST", "$DATABASE_ID"], 1]],
                        ["COND",
                            ["GREATER_EQUAL", "$I", "$COUNT"],
                            ["UPDATE_VAR", "$I", 0]
                        ],
                        "$LIST.[$I]"
                    ]
                ]
            ]
        },
        //generic function returning next element on the list after the given element (or first element for null) that has no status set, or null
        "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_ID": {
            "args": ["$VARIABLE_TEMPLATE", "$DATABASE_ID", "$DEFAULT", "$LIST"],
            "code": [
                ["VALIDATE_NOT_EMPTY", "$VARIABLE_TEMPLATE", "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_ID.VARIABLE_TEMPLATE"],
                //$DATABASE_ID may be null
                ["VALIDATE_NOT_EMPTY", "$DEFAULT", "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_ID.DEFAULT"],
                ["VALIDATE_GT0", ["LENGTH", "$LIST"], "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_ID.LIST"],
                ["VAR", "$CONTINUE", true],
                ["VAR", "$CURRENT_ID", ["EDGE_OF_THE_EARTH_GET_NEXT_ID", "$DATABASE_ID", "$LIST"]],
                ["VAR", "$VARIABLE", ["FORMAT", "$VARIABLE_TEMPLATE", "$CURRENT_ID"]],
                ["WHILE",
                    ["EQUAL", "$CONTINUE", true],
                    [
                        ["COND",
                            ["DEFINED", "$GAME.arkham.{{$VARIABLE}}"],
                            [
                                ["UPDATE_VAR", "$CURRENT_ID", ["EDGE_OF_THE_EARTH_GET_NEXT_ID", "$CURRENT_ID", "$LIST"]],
                                ["UPDATE_VAR", "$VARIABLE", ["FORMAT", "$VARIABLE_TEMPLATE", "$CURRENT_ID"]],
                                ["COND",
                                    ["EQUAL", "$CURRENT_ID", "$LIST.[0]"],
                                    [
                                        ["UPDATE_VAR", "$CURRENT_ID", null],
                                        ["UPDATE_VAR", "$CONTINUE", false]
                                    ]
                                ]
                            ],
                            ["TRUE"],
                            ["UPDATE_VAR", "$CONTINUE", false]
                        ]
                    ]
                ],
                "$CURRENT_ID"
            ]
        },
        //generic function initiating asking for card status
        "EDGE_OF_THE_EARTH_ASK_FOR_CARD_STATUS": {
            "args": ["$TARGET_PLAYER", "$UNDEFINED_FUNCTION", "$PROMPT", "$CONTINUE_ACTION"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "EDGE_OF_THE_EARTH_ASK_FOR_CARD_STATUS.TARGET_PLAYER"],
                ["VALIDATE_NOT_EMPTY", "$UNDEFINED_FUNCTION", "EDGE_OF_THE_EARTH_ASK_FOR_CARD_STATUS.UNDEFINED_FUNCTION"],
                ["VALIDATE_NOT_EMPTY", "$PROMPT", "EDGE_OF_THE_EARTH_ASK_FOR_CARD_STATUS.PROMPT"],
                ["VALIDATE_NOT_EMPTY", "$CONTINUE_ACTION", "EDGE_OF_THE_EARTH_ASK_FOR_CARD_STATUS.CONTINUE_ACTION"],
                ["VAR", "$DATABASE_ID", ["$UNDEFINED_FUNCTION", null]],
                ["COND",
                    ["EQUAL", "$DATABASE_ID", null],
                    ["ACTION_LIST", "$CONTINUE_ACTION"],
                    ["TRUE"],
                    ["PROMPT", "$TARGET_PLAYER", "$PROMPT", "$DATABASE_ID", ["GET_CARD_TITLE", "$CARD_DB.{{$DATABASE_ID}}.A.name"], "$CONTINUE_ACTION"]
                ]
            ]
        },
        //generic function continuing asking for card status
        "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE": {
            "args": ["$TARGET_PLAYER", "$UNDEFINED_FUNCTION", "$SET_FUNCTION", "$PROMPT", "$CONTINUE_ACTION", "$DATABASE_ID", "$STATUS"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.TARGET_PLAYER"],
                ["VALIDATE_NOT_EMPTY", "$UNDEFINED_FUNCTION", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.UNDEFINED_FUNCTION"],
                ["VALIDATE_NOT_EMPTY", "$SET_FUNCTION", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.SET_FUNCTION"],
                ["VALIDATE_NOT_EMPTY", "$PROMPT", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.PROMPT"],
                ["VALIDATE_NOT_EMPTY", "$CONTINUE_ACTION", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.CONTINUE_ACTION"],
                ["VALIDATE_NOT_EMPTY", "$DATABASE_ID", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.DATABASE_ID"],
                ["VALIDATE_NOT_EMPTY", "$STATUS", "EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE.STATUS"],
                ["LOGF", "{0} states {1} is {2}.", "$TARGET_PLAYER", ["GET_CARD_TITLE", "$CARD_DB.{{$DATABASE_ID}}.A.name"], "$STATUS"],
                ["$SET_FUNCTION", "$DATABASE_ID", "$STATUS"],
                ["VAR", "$NEXT_DATABASE_ID", ["$UNDEFINED_FUNCTION", "$DATABASE_ID"]],
                ["COND",
                    ["EQUAL", "$NEXT_DATABASE_ID", null],
                    ["ACTION_LIST", "$CONTINUE_ACTION"],
                    ["TRUE"],
                    ["PROMPT", "$TARGET_PLAYER", "$PROMPT", "$NEXT_DATABASE_ID", ["GET_CARD_TITLE", "$CARD_DB.{{$NEXT_DATABASE_ID}}.A.name"], "$CONTINUE_ACTION"]
                ]
            ]
        },
        //selecting location status 1/5
        "EDGE_OF_THE_EARTH_GET_LOCATION_STATUS": {
            "args": ["$DATABASE_ID"],
            "code": [
                ["EDGE_OF_THE_EARTH_GET_STATUS", "campaignEdgeOfTheEarthLocation{0}Status", "$DATABASE_ID", "unrevealed"]
            ]
        },
        //selecting location status 2/5
        "EDGE_OF_THE_EARTH_SET_LOCATION_STATUS": {
            "args": ["$DATABASE_ID", "$STATUS"],
            "code": [
                ["EDGE_OF_THE_EARTH_SET_STATUS", "campaignEdgeOfTheEarthLocation{0}Status", "$DATABASE_ID", "$STATUS"]
            ]
        },
        //selecting location status 3/5
        "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_LOCATION_DATABASE_ID": {
            "args": ["$DATABASE_ID"],
            "code": [
                ["EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_ID", "campaignEdgeOfTheEarthLocation{0}Status", "$DATABASE_ID", "unrevealed", ["LIST", "08503", "08504", "08505", "08506", "08507", "08508", "08509", "08510", "08511", "08512", "08513", "08514"]] //Frozen Shores, Treacherous Path, Precarious Ice Sheet, Broad Snowdrifts, Icy Wastes, Rocky Crags, Snow Graves, Icebreaker Landing, Frigid Cave, Barrier Camp, Remnants of Lake's Camp, Crystalline Cavern
            ]
        },
        //selecting location status 4/5
        "EDGE_OF_THE_EARTH_ASK_FOR_LOCATION_STATUS": {
            "args": ["$CONTINUE_ACTION"],
            "code": [
                ["EDGE_OF_THE_EARTH_SET_LOCATION_STATUS", "08502", "revealed"], //Crash Site
                ["EDGE_OF_THE_EARTH_ASK_FOR_CARD_STATUS", "$GAME.firstPlayer", "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_LOCATION_DATABASE_ID", "edgeOfTheEarthChooseLocationStatus", "$CONTINUE_ACTION"]
            ]
        },
        //selecting location status 5/5
        "EDGE_OF_THE_EARTH_LOCATION_STATUS_CHOICE_DONE": {
            "args": ["$TARGET_PLAYER", "$CONTINUE_ACTION", "$DATABASE_ID", "$STATUS"],
            "code": [
                ["EDGE_OF_THE_EARTH_CARD_STATUS_CHOICE_DONE", "$TARGET_PLAYER", "EDGE_OF_THE_EARTH_GET_NEXT_UNDEFINED_LOCATION_DATABASE_ID", "EDGE_OF_THE_EARTH_SET_LOCATION_STATUS", "edgeOfTheEarthChooseLocationStatus", "$CONTINUE_ACTION", "$DATABASE_ID", "$STATUS"]
            ]
        },
        //flips faceup all eligible locations that have "revealed" status, blocks placing clues on camp location in Ice and Death Pt 3
        "EDGE_OF_THE_EARTH_REVEAL_LOCATIONS": {
            "args": [],
            "code": [
                ["VAR", "$LOCATIONS", ["LIST", "08502", "08503", "08504", "08505", "08506", "08507", "08508", "08509", "08510", "08511", "08512", "08513", "08514"]], //Crash Site, Frozen Shores, Treacherous Path, Precarious Ice Sheet, Broad Snowdrifts, Icy Wastes, Rocky Crags, Snow Graves, Icebreaker Landing, Frigid Cave, Barrier Camp, Remnants of Lake's Camp, Crystalline Cavern
                ["FOR_EACH_VAL", "$DATABASE_ID", "$LOCATIONS", [
                    ["COND",
                        ["EQUAL", ["EDGE_OF_THE_EARTH_GET_LOCATION_STATUS", "$DATABASE_ID"], "revealed"],
                        [
                            ["VAR", "$LOCATION_ID", ["GET_NAMED_CARD_ID", "$DATABASE_ID"]],
                            ["DO_FLIP_FACEUP", "$GAME.firstPlayer", "$LOCATION_ID", true, null],
                            ["COND",
                                ["AND",
                                    ["EQUAL", "$GAME.arkham.scenarioName", "Ice and Death Pt 3"],
                                    ["EQUAL", "$DATABASE_ID", "$GAME.arkham.campaignEdgeOfTheEarthCamp"],
                                    ["GREATER_THAN", ["GET_CARD_CLUES_REQUIREMENT", "$LOCATION_ID"], 0]
                                ],
                                ["SET", "/cardById/$LOCATION_ID/cluesPlaced", true]
                            ]
                        ]
                    ]
                ]]
            ]
        },
        //selecting partner status 1/5
        "EDGE_OF_THE_EARTH_GET_PARTNER_STATUS": {
            "args": ["$DATABASE_ID"],
            "code": [
                ["EDGE_OF_THE_EARTH_GET_STATUS", "campaignEdgeOfTheEarthPartner{0}Status", "$DATABASE_ID", "alive"]
            ]
        },
        //selecting partner status 2/5
        "EDGE_OF_THE_EARTH_SET_PARTNER_STATUS": {
            "args": ["$DATABASE_ID", "$STATUS"],
            "code": [
