                ["EDGE_OF_THE_EARTH_ADD_PARTNER_CHOICE", "$TARGET_PLAYER", "08720", "7", "$CONTINUE_ACTION"], //Dr. Amy Kensler: Professor of Biology
                ["EDGE_OF_THE_EARTH_ADD_PARTNER_CHOICE", "$TARGET_PLAYER", "08721", "8", "$CONTINUE_ACTION"], //Roald Ellsworth: Intrepid Explorer
