    "actionLists": {
        "Machinations Through Time Scenario Early Init": [
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Decide.", ["LIST", "Play in Epic Multiplayer Mode (Past).", "Play in Epic Multiplayer Mode (Present).", "Play in Epic Multiplayer Mode (Future).", "Play in Single Group Mode."], "Machinations Through Time Scenario Question 1"]
        ],
        "Machinations Through Time Scenario Question 1": [
            ["LOG", "└── Decision: {{$GAME.arkham.scenarioAnswer1Text}}"],
            ["SET", "/arkham/scenarioMachinationsThroughTimePast", ["OR", ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"], ["EQUAL", "$GAME.arkham.scenarioAnswer1", "D"]]],
            ["SET", "/arkham/scenarioMachinationsThroughTimePresent", ["OR", ["EQUAL", "$GAME.arkham.scenarioAnswer1", "B"], ["EQUAL", "$GAME.arkham.scenarioAnswer1", "D"]]],
            ["SET", "/arkham/scenarioMachinationsThroughTimeFuture", ["OR", ["EQUAL", "$GAME.arkham.scenarioAnswer1", "C"], ["EQUAL", "$GAME.arkham.scenarioAnswer1", "D"]]],
            ["SET", "/arkham/scenarioMachinationsThroughTimeEpicMultiplayerMode", ["NOT_EQUAL", "$GAME.arkham.scenarioAnswer1", "D"]],
            ["COND",
                ["NOT_EQUAL", "$GAME.arkham.scenarioAnswer1", "D"],
                ["CALCULATOR_PROMPT", "$GAME.firstPlayer", "Enter number of players", ["ADD", "$GAME.arkham.numPlayers", 2], 12, 0, "MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK", "$GAME.firstPlayer", "", ""],
                ["TRUE"],
                ["MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK", "$GAME.firstPlayer", "", "└── ", "$GAME.firstPlayer", "{{$GAME.arkham.numPlayers}}"]
            ]
        ],
        "Machinations Through Time Scenario Init": [
            ["VAR", "$HEALTH", ["MULTIPLY", "$GAME.arkham.scenarioMachinationsThroughTimeNumberOfPlayers", 6]],
            ["DO_LOAD_CARDS_SCN", "Machinations Through Time"],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "A"],
                ["DO_LOAD_CARDS_SCN", "Machinations Through Time - Epic Multiplayer Mode (Past)"],
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "B"],
                ["DO_LOAD_CARDS_SCN", "Machinations Through Time - Epic Multiplayer Mode (Present)"],
                ["EQUAL", "$GAME.arkham.scenarioAnswer1", "C"],
                [
                    ["DO_LOAD_CARDS_SCN", "Machinations Through Time - Epic Multiplayer Mode (Future)"],
                    ["DO_SET_SPAWN_LOCATION", "87029", 45, 79] //Corrigan Industries
                ],
                ["TRUE"],
                [
                    ["DO_LOAD_CARDS_SCN", "Machinations Through Time - Single Group Mode"],
                    ["DO_SET_SPAWN_LOCATION", "87029", 80, 79] //Corrigan Industries
                ]
            ],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true],
                [
                    ["VAR", "$TINDALOS", ["GET_NAMED_MAP_LOCATION", "Tindalos"]],
                    ["DO_FLIP_FACEDOWN", "$GAME.firstPlayer", "$TINDALOS.id", false, null]
                ]
            ],
            ["DO_SPAWN_MINI_INVESTIGATORS_AT_LOCATION", "Tindalos", 2, 7, 2, 0],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePast", true],
                ["MACHINATIONS_THROUGH_TIME_A_NOBLE_LEGACY_PAST"]
            ],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePresent", true],
                ["MACHINATIONS_THROUGH_TIME_A_NOBLE_LEGACY_PRESENT"]
            ],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeFuture", true],
                ["MACHINATIONS_THROUGH_TIME_A_NOBLE_LEGACY_FUTURE"]
            ],
            ["VAR", "$MACHINATION", ["SHUFFLE_LIST", ["LIST", "MACHINATIONS_THROUGH_TIME_MACHINATION_A_BITTER_RIVALRY", "MACHINATIONS_THROUGH_TIME_MACHINATION_REDEEM_A_FORMER_COLLEAGUE", "MACHINATIONS_THROUGH_TIME_MACHINATION_UNEASY_ALLIANCE"]]],
            ["$MACHINATION.[0]"],
            ["VAR", "$PLOT", ["SHUFFLE_LIST", ["LIST", "MACHINATIONS_THROUGH_TIME_PLOT_ANOMALIES_IN_SPACETIME", "MACHINATIONS_THROUGH_TIME_PLOT_MOB_TROUBLES", "MACHINATIONS_THROUGH_TIME_PLOT_UNSPEAKABLE_ABOMINATION"]]],
            ["$PLOT.[0]"],
            ["VAR", "$DOOM", "$GAME.arkham.numPlayers"],
            ["COND",
                ["EQUAL", "$GAME.arkham.scenarioDifficulty", "Easy"],
                ["DECREASE_VAR", "$DOOM", 1],
                ["EQUAL", "$GAME.arkham.scenarioDifficulty", "Hard"],
                ["INCREASE_VAR", "$DOOM", 1],
                ["EQUAL", "$GAME.arkham.scenarioDifficulty", "Expert"],
                ["INCREASE_VAR", "$DOOM", 2]
            ],
            ["COND",
                ["GREATER_THAN", "$DOOM", 0],
                ["DO_PLACE_DOOM_ON_CURRENT_AGENDA_SCN", "$DOOM"]
            ],
            ["SCENARIO_INIT_CONTINUE", null]
        ]
    },
    "functions": {
        "MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK": {
            "args": ["$TARGET_PLAYER", "$TARGET_CARD_ID", "$ARGS", "$ACTOR_PLAYER", "$SELECTED_VALUE"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK.TARGET_PLAYER"],
                ["VALIDATE_NOT_NULL", "$ARGS", "MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK.ARGS"],
                ["VALIDATE_NOT_EMPTY", "$SELECTED_VALUE", "MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK.SELECTED_VALUE"],
                ["VAR", "$VALUE", ["CALC", "{{$SELECTED_VALUE}}"]],
                ["VALIDATE_GT0", "$VALUE", "MACHINATIONS_THROUGH_TIME_PLAYERS_CALLBACK.SELECTED_VALUE"],
                ["SET", "/arkham/scenarioMachinationsThroughTimeNumberOfPlayers", "$VALUE"],
                ["LOGF", "{0}{1} set number of players to {2}.", "$ARGS", "$TARGET_PLAYER", "$VALUE"],
                ["SCENARIO_INIT_RESUME", "Machinations Through Time Scenario Init"]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_ID": {
            "args": [],
            "code": [
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true],
                    "87037a", //Edwin Bennet: Envious Rival
                    ["TRUE"],
                    "87036a"  //Edwin Bennet: Bitter Adversary
                ]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_NAME": {
            "args": ["$ENEMY"],
            "code": [
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true],
                    ["INLINE_IF", ["EQUAL", "$ENEMY", true], "Edwin Bennet: Envious Rival", "Edwin Bennet: Esteemed Colleague"],
                    ["TRUE"],
                    ["INLINE_IF", ["EQUAL", "$ENEMY", true], "Edwin Bennet: Bitter Adversary", "Edwin Bennet: Astute Associate"]
                ]
            ]
        },
