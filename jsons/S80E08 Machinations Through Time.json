            ]
        },
        "MACHINATIONS_THROUGH_TIME_MACHINATION_A_BITTER_RIVALRY": {
            "args": [],
            "code": [
                //Follow the instructions for your relevant era(s), below.
                //Past: Set Thoman Corrigan aside, out of play; he has been abducted. Place Mary Zielinski at Arkham Gazette. Set the Edwin Bennet enemy aside, out of play.
                //Present and Future: Set Thomas Corrigan and Mary Zielinski aside, out of play; they have been abducted. If you are playing in Epic Multiplayer Mode, set the Edwin Bennet enemy aside, out of play.
                ["DO_CREATE_CARDS_SCN", "A Bitter Rivalry", "87033", 1, "sharedOffMap"],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePast", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Physics Student", "87012", 1, "sharedNotInPlay"],
                        ["DO_SPAWN_AT_LOCATION_SCN", "Mary Zielinski: Chemistry Student", "87013", "87007", 3, 3] //Arkham Gazette
                    ]
                ],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePresent", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Preeminent Astrophysicist", "87021", 1, "sharedNotInPlay"],
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Distinguished Chemist", "87022", 1, "sharedNotInPlay"]
                    ]
                ],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeFuture", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Cofounder of Corrigan Industries", "87030", 1, "sharedNotInPlay"],
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Cofounder of Corrigan Industries", "87031", 1, "sharedNotInPlay"]
                    ]
                ],
                ["COND",
                    ["OR",
                        ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePast", true],
                        ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true]
                    ],
                    ["DO_CREATE_CARDS_SCN", ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_NAME", true], ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_ID"], 1, "sharedNotInPlay"]
                ]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_MACHINATION_REDEEM_A_FORMER_COLLEAGUE": {
            "args": [],
            "code": [
                //Follow the instructions for your relevant era(s), below. Then, flip this story card over.
                //Past: Set Mary Zielinski aside, out of play; she has been abducted. Place Thomas Corrigan at Childhood Home. If you are playing in Epic Multiplayer Mode, set the Edwin Bennet enemy aside, out of play.
                //Present: Set Thomas Corrigan and Mary Zielinsky aside, out of play: they have been abducted. Spawn the Edwin Bennet enemy at Miskatonic University.
                //Future: Set Thomas Corrigan and Mary Zielinski aside, out of play; they have been abducted. If you are playing in Epic Multiplayer Mode, set the Edwin Bennet enemy aside, out of play.
                ["DO_CREATE_CARDS_SCN", "Redeem a Former Colleague", "87034", 1, "sharedOffMap"],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePast", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Chemistry Student", "87013", 1, "sharedNotInPlay"],
                        ["DO_SPAWN_AT_LOCATION_SCN", "Thomas Corrigan: Physics Student", "87012", "87011", 3, 3] //Childhood Home
                    ]
                ],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePresent", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Preeminent Astrophysicist", "87021", 1, "sharedNotInPlay"],
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Distinguished Chemist", "87022", 1, "sharedNotInPlay"],
                        ["DO_SPAWN_AT_LOCATION_SCN", ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_NAME", true], ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_ID"], "87019", 3, 3] //Miskatonic University (Present)
                    ]
                ],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeFuture", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Cofounder of Corrigan Industries", "87030", 1, "sharedNotInPlay"],
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Cofounder of Corrigan Industries", "87031", 1, "sharedNotInPlay"]
                    ]
                ],
                ["COND",
                    ["AND",
                        ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true],
                        ["NOT_EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePresent", true]
                    ],
                    ["DO_CREATE_CARDS_SCN", ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_NAME", true], ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_ID"], 1, "sharedNotInPlay"]
                ],
                ["VAR", "$MACHINATION_ID", ["GET_NAMED_CARD_ID", "Redeem a Former Colleague"]],
                ["DO_FLIP_FACEDOWN", "$GAME.firstPlayer", "$MACHINATION_ID", true, null]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_MACHINATION_UNEASY_ALLIANCE": {
            "args": [],
            "code": [
                //Follow the instructions for your relevant era(s), below. Then, flip this story card over.
                //Past: Place Thomas Corrigan at Childhood Home. Place Mary Zielinski at O'Malley's Watch Shop. Place the Edwin Bennet asset at Arkham Gazette with 12 clues on him, then remove 3(inv) clues from him.
                //Present and Future: Set Thomas Corrigan and Mary Zielinsky aside, out of play: they have been abducted. If you are playing in Epic Multiplayer Mode, set the Edwin Bennet asset aside, out of play.
                ["DO_CREATE_CARDS_SCN", "Uneasy Alliance", "87035", 1, "sharedOffMap"],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePast", true],
                    [
                        ["DO_SPAWN_AT_LOCATION_SCN", "Thomas Corrigan: Physics Student", "87012", "87011", 3, 3], //Childhood Home
                        ["DO_SPAWN_AT_LOCATION_SCN", "Mary Zielinski: Chemistry Student", "87013", "87008", 3, 3], //O'Malley's Watch Shop
                        ["DO_SPAWN_AT_LOCATION_SCN", ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_NAME", false], ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_ID"], "87007", 3, 3], //Arkham Gazette
                        ["MACHINATIONS_THROUGH_TIME_FLIP_EDWIN_BENNET"],
                        ["VAR", "$CLUES", ["SUBTRACT", 12, ["MULTIPLY", "$GAME.arkham.numPlayers", 3]]],
                        ["COND",
                            ["GREATER_THAN", "$CLUES", 0],
                            ["DO_PLACE_CLUES_ON_CARD", "$GAME.firstPlayer", ["GET_NAMED_CARD_ID", "Edwin Bennet"], "$CLUES", true, null]
                        ]
                    ]
                ],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePresent", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Preeminent Astrophysicist", "87021", 1, "sharedNotInPlay"],
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Distinguished Chemist", "87022", 1, "sharedNotInPlay"]
                    ]
                ],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeFuture", true],
                    [
                        ["DO_CREATE_CARDS_SCN", "Thomas Corrigan: Cofounder of Corrigan Industries", "87030", 1, "sharedNotInPlay"],
                        ["DO_CREATE_CARDS_SCN", "Mary Zielinski: Cofounder of Corrigan Industries", "87031", 1, "sharedNotInPlay"]
                    ]
                ],
                ["COND",
                    ["AND",
                        ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true],
                        ["NOT_EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePast", true]
                    ],
                    [
                        ["DO_CREATE_CARDS_SCN", ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_NAME", false], ["MACHINATIONS_THROUGH_TIME_EDWIN_BENNET_ID"], 1, "sharedNotInPlay"],
                        ["MACHINATIONS_THROUGH_TIME_FLIP_EDWIN_BENNET"]
                    ]
                ],
                ["VAR", "$MACHINATION_ID", ["GET_NAMED_CARD_ID", "Uneasy Alliance"]],
                ["DO_FLIP_FACEDOWN", "$GAME.firstPlayer", "$MACHINATION_ID", true, null]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_PLOT_ANOMALIES_IN_SPACETIME": {
            "args": [],
            "code": [
                //Place 1(inv) horror on each Miskatonic University location, as anomalies. If you are playing in Epic Multiplayer Mode, also place 1(inv) horror on the following locations, as anomalies: Arkham Gazette, Arkham Advertiser, O'Malley's Watch Shop, and Tick-Tock Club.
                //Remove the following enemies from the game: Tyr'thrha, Old Sadie Sheldon, and all 3 copies of Sheldon Gang. Flip this story card over.
                ["DO_CREATE_CARDS_SCN", "Anomalies in Spacetime", "87038", 1, "sharedOffMap"],
                ["VAR", "$LOCATIONS", ["LIST", "Miskatonic University"]],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimeEpicMultiplayerMode", true],
                    ["UPDATE_VAR", "$LOCATIONS", ["CONCAT_LISTS", "$LOCATIONS", ["LIST", "Arkham Gazette", "Arkham Advertiser", "O'Malley's Watch Shop", "Tick-Tock Club"]]]
                ],
                ["FOR_EACH_VAL", "$LOCATION", "$LOCATIONS", [
                    ["VAR", "$SELECTED_CARDS", ["GET_NAMED_CARDS", "$LOCATION"]],
                    ["COND",
                        ["GREATER_THAN", ["LENGTH", "$SELECTED_CARDS"], 0],
                        [
                            ["VAR", "$HORROR", "$GAME.arkham.numPlayers"],
                            ["FOR_EACH_VAL", "$CARD", "$SELECTED_CARDS", [
                                ["DO_PLACE_HORROR_ON_CARD", "$GAME.firstPlayer", "$CARD.id", "$HORROR", true, null]
                            ]]
                        ]
                    ]
                ]],
                ["VAR", "$PLOT_ID", ["GET_NAMED_CARD_ID", "Anomalies in Spacetime"]],
                ["DO_FLIP_FACEDOWN", "$GAME.firstPlayer", "$PLOT_ID", true, null]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_PLOT_MOB_TROUBLES": {
            "args": [],
            "code": [
                //Set the Old Sadie Sheldon enemy and all 3 copies of the Sheldon Gang enemy aside, out of play. Remove the Tyr'thrha enemy from the game. You make a deal with Old Sadie Sheldon: each investigator playing the present era gains 2 resources.
                ["DO_CREATE_CARDS_SCN", "Mob Troubles", "87039", 1, "sharedOffMap"],
                ["DO_CREATE_CARDS_SCN", "Old Sadie Sheldon: Bootlegging Kingpin", "87040", 1, "sharedNotInPlay"],
                ["DO_CREATE_CARDS_SCN", "Sheldon Gang", "87041", 3, "sharedNotInPlay"],
                ["COND",
                    ["EQUAL", "$GAME.arkham.scenarioMachinationsThroughTimePresent", true],
                    [
                        ["FOR_EACH_VAL", "$PLAYER_I", "$PLAYER_ORDER", [
                            ["DO_GAIN_RESOURCES", "$PLAYER_I", 2, true, null] //resources are given directly, as this happens after starting resources have been collected
                        ]]
                    ]
                ]
            ]
        },
        "MACHINATIONS_THROUGH_TIME_PLOT_UNSPEAKABLE_ABOMINATION": {
            "args": [],
            "code": [
                //Set the Tyr'thrha enemy aside, out of play. Remove the Old Sadie Sheldon enemy and all 3 copies of the Sheldon Gang enemy from the game.
                ["DO_CREATE_CARDS_SCN", "Unspeakable Abomination", "87042", 1, "sharedOffMap"],
                ["DO_CREATE_CARDS_SCN", "Tyr'thrha: Arch-Lord of Tindalos", "87043", 1, "sharedNotInPlay"],
                ["VAR", "$HEALTH", ["MULTIPLY", "$GAME.arkham.scenarioMachinationsThroughTimeNumberOfPlayers", 6]],
                ["VAR", "$TYRTHRHA_ID", ["GET_NAMED_CARD_ID", "Tyr'thrha"]],
                ["SET", "/cardById/$TYRTHRHA_ID/sides/A/health", "$HEALTH"],
                ["SET", "/cardById/$TYRTHRHA_ID/sides/A/healthPerInvestigator", 0],
                ["SET", "/arkham/finalScenarioMessage", ["FORMAT", "Tyr'thrha's health: {0}.", "$HEALTH"]]
            ]
        }
    }
}