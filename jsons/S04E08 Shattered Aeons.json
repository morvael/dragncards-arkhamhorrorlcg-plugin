            ["COND",
                ["GREATER_THAN", "$CARDS_TO_MOVE", 0],
                [
                    ["DO_MOVE_DECK_CARDS_SCN", "Player Card", ["GET_CONTROLLER_DECK", "$GAME.firstPlayer"], "$CARDS_TO_MOVE", "sharedVictoryDisplay"],
                    ["VAR", "$SELECTED_CARDS", ["FILTER_CARDS", "$CARD", ["AND",
