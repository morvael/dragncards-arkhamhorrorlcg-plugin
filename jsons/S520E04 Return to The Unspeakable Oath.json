    "actionLists": {
        "Return to The Unspeakable Oath Scenario Init": [
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Check Campaign Log.", ["LIST", "Constance Dumaine is listed under VIPs Interviewed.", "The above is not true."], "Return to The Unspeakable Oath Scenario Question 1"]
        ],
        "Return to The Unspeakable Oath Scenario Question 1": [
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer1", "A"],
                [
                    ["LOG", "└── Campaign Log: Constance Dumaine is listed under VIPs Interviewed."],
                    ["FOR_EACH_VAL", "$PLAYER_I", "$PLAYER_ORDER", [
                        ["VAR", "$TOP_CARD", ["GET_TOP_CARD", ["GET_CONTROLLER_DECK", "$PLAYER_I"]]],
                        ["DO_LOG_SCN", "$PLAYER_I", "places the top card of their deck facedown in their play area as Courage asset."],
                        ["MOVE_CARD", "$TOP_CARD.id", ["GET_CONTROLLER_PLAY", "$PLAYER_I"], -1],
                        ["SET", "/cardById/$TOP_CARD.id/currentSide", "B"]
                    ]]
                ]
            ],
            ["DO_LOAD_CARDS_SCN", "Return to The Unspeakable Oath"],
            ["DO_LOAD_CARDS_SCN", "Hastur's Gift"],
            ["DO_LOAD_CARDS_SCN", "Inhabitants of Carcosa"],
            ["DO_LOAD_CARDS_SCN", "Maddening Delusions"],
            ["DO_LOAD_CARDS_SCN", "Decaying Reality"],
            ["DO_LOAD_CARDS_SCN", "Hastur's Envoys"],
            ["VAR", "$MONSTERS", ["FILTER_CARDS", "$CARD", ["AND",
                ["EQUAL", "$CARD.groupId", "sharedEncounterDeck"],
                ["EQUAL", "$CARD.sides.A.type", "Enemy"],
                ["IN_STRING", "$CARD.sides.A.traits", "Monster."]
            ]]],
            ["FOR_EACH_VAL", "$CARD", "$MONSTERS", [
                ["MOVE_CARD", "$CARD.id", "sharedDeckA", -1]
            ]],
            ["VAR", "$POSSESSED", ["FILTER_CARDS", "$CARD", ["AND",
                ["EQUAL", "$CARD.groupId", "sharedEncounterDeck"],
                ["EQUAL", "$CARD.sides.A.type", "Enemy"],
                ["OR",
                    ["IN_STRING", "$CARD.sides.A.traits", "Possessed."],
                    ["IN_STRING", "$CARD.sides.A.traits", "Lunatic."]
                ]
            ]]],
            ["FOR_EACH_VAL", "$CARD", "$POSSESSED", [
                ["MOVE_CARD", "$CARD.id", "sharedDeckB", -1]
            ]],
            ["DO_LOAD_N_CARDS_SCN", "Return to The Unspeakable Oath - Western Patient Wing", 1],
            ["DO_LOAD_N_CARDS_SCN", "Return to The Unspeakable Oath - Eastern Patient Wing", 1],
            ["SET", "/arkhamReturnToTheUnspeakableOathSpawnChoiceDone", 0],
            ["FOR_EACH_VAL", "$PLAYER_I", "$PLAYER_ORDER", [
                ["PROMPT", "$PLAYER_I", "returnToTheUnspeakableOathSpawnChoice"]
            ]]
        ],
        "Return to The Unspeakable Oath Scenario Init Continue 1": [
            ["DO_SPAWN_MINI_INVESTIGATORS_AT_LOCATION", "Asylum Halls: Western Patient Wing", 5, 7, 3, 0],
            ["DO_LOAD_CARDS_SCN", "Return to The Unspeakable Oath - Asylum"],
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioDifficulty", "Easy"],
                ["DO_ADD_CHAOS_TOKEN_SCN", "-2", "chaos_m2"],
                ["EQUAL", "$GAME.arkhamScenarioDifficulty", "Normal"],
                ["DO_ADD_CHAOS_TOKEN_SCN", "-3", "chaos_m3"],
                ["EQUAL", "$GAME.arkhamScenarioDifficulty", "Hard"],
                ["DO_ADD_CHAOS_TOKEN_SCN", "-4", "chaos_m4"],
                ["EQUAL", "$GAME.arkhamScenarioDifficulty", "Expert"],
                ["DO_ADD_CHAOS_TOKEN_SCN", "-5", "chaos_m5"]
            ],
            ["ASK_SCENARIO_QUESTION_AND_CONTINUE", "Check Campaign Log.", ["LIST", "You have more (or equal) Conviction than Doubt.", "You have more Doubt than Conviction."], "Return to The Unspeakable Oath Scenario Question 2"]
        ],
        "Return to The Unspeakable Oath Scenario Question 2": [
            ["COND",
                ["EQUAL", "$GAME.arkhamScenarioAnswer2", "A"],
                [
                    ["LOG", "└── Campaign Log: You have more (or equal) Conviction than Doubt."],
                    ["DO_CREATE_STORY_CARD_SCN", "\"The Really Bad Ones\" (v. III)", "52035", 1, "sharedActStory"]
                ],
                ["EQUAL", "$GAME.arkhamScenarioAnswer2", "B"],
                [
                    ["LOG", "└── Campaign Log: You have more Doubt than Conviction."],
                    ["DO_CREATE_STORY_CARD_SCN", "\"The Really Bad Ones\" (v. IV)", "52036", 1, "sharedActStory"]
                ]
            ],
            ["SET", "/arkhamFinalScenarioMessage", "Monster enemy cards are in Deck A, Possessed (Lunatic) enemy cards are in Deck B, Patient Confinement cards are in Deck C."],
            ["DO_ATTACH_REFERENCE_TO_SCENARIO", false],
            ["SCENARIO_INIT_CONTINUE", null]
        ]
    },
    "functions": {
        "DO_RETURN_TO_THE_UNSPEAKABLE_OATH_SPAWN_CHOICE": {
            "args": ["$TARGET_PLAYER", "$LOCATION_CARD_NAME"],
            "code": [
                ["VALIDATE_PLAYER", "$TARGET_PLAYER", "DO_RETURN_TO_THE_UNSPEAKABLE_OATH_SPAWN_CHOICE.TARGET_PLAYER"],
                ["VALIDATE_NOT_EMPTY", "$LOCATION_CARD_NAME", "DO_RETURN_TO_THE_UNSPEAKABLE_OATH_SPAWN_CHOICE.LOCATION_CARD_NAME"],
                ["SET", "/playerData/$TARGET_PLAYER/arkhamCustomSpawnLocation", "$LOCATION_CARD_NAME"],
                ["INCREASE_VAL", "/arkhamReturnToTheUnspeakableOathSpawnChoiceDone", 1],
                ["COND",
                    ["GREATER_EQUAL", "$GAME.arkhamReturnToTheUnspeakableOathSpawnChoiceDone", "$GAME.numPlayers"],
                    ["ACTION_LIST", "Return to The Unspeakable Oath Scenario Init Continue 1"]
                ]
            ]
        }
    },
    "prompts": {
        "returnToTheUnspeakableOathSpawnChoice": {
            "args": [],
            "message": "Where do you want to begin the game?",
            "options": [
                {
                    "label": "Western Patient Wing.",
                    "hotkey": "W",
                    "code": [
                        ["DO_RETURN_TO_THE_UNSPEAKABLE_OATH_SPAWN_CHOICE", "$PLAYER_N", "Asylum Halls: Western Patient Wing"]
                    ]
                },
                {
                    "label": "Eastern Patient Wing.",
                    "hotkey": "E",
                    "code": [
                        ["DO_RETURN_TO_THE_UNSPEAKABLE_OATH_SPAWN_CHOICE", "$PLAYER_N", "Asylum Halls: Eastern Patient Wing"]
                    ]
                }
            ]
        }
    }
